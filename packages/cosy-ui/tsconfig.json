{
    "extends": "astro/tsconfigs/strict",
    "include": [
        ".astro/types.d.ts",
        "**/*"
    ],
    "exclude": [
        "dist",
        "dist-docs"
    ],
    "compilerOptions": {
        "baseUrl": ".",
        "jsx": "preserve",
        "jsxImportSource": "astro",
        "allowJs": true,
        "types": [
            "astro/client"
        ]
    }
}