{
	"name": "@coffic/cosy-ui",
	"version": "0.9.83",
	"description": "An astro component library",
	"author": {
		"name": "nookery",
		"url": "https://github.com/nookery"
	},
	"repository": {
		"url": "https://github.com/CosyZone/cosy-ui"
	},
	"license": "MIT",
	"keywords": [
		"astro-integration",
		"astro-component",
		"withastro",
		"astro",
		"cosy-ui"
	],
	"homepage": "https://ui.coffic.cn/en",
	"publishConfig": {
		"access": "public"
	},
	"sideEffects": [
