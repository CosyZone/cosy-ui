    "@tsconfig/node22": "^22.0.2",
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^24.9.2",
    "astro": "^5.15.2",
    "daisyui": "^5.3.10",
    "globals": "^16.4.0",
    "rollup-plugin-copy": "^3.5.0",
