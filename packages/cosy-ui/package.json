		"@tsconfig/node22": "^22.0.2",
		"@types/fs-extra": "^11.0.4",
		"@types/node": "^24.8.1",
		"astro": "^5.14.5",
		"daisyui": "^5.0.50",
		"globals": "^16.3.0",
		"rollup-plugin-copy": "^3.5.0",
		"sharp": "^0.33.5",
		"tailwindcss": "^4.1.11",
		"tsx": "^4.20.3",
		"typescript": "^5.9.2",
		"vite": "^7.1.10"
