---
import "../../style.ts";
import { Alert } from "../../index-astro";

interface Props {
	messages: string[];
}

const { messages } = Astro.props;
---

<section class="cosy:w-full cosy:mx-auto" aria-label="container-error">
    <Alert type="warning" variant="soft" closable={false}>
        <ul class="cosy:list-disc cosy:pl-5 cosy:space-y-1">
            {messages.map((message) => <li class="cosy:text-sm">{message}</li>)}
        </ul>
    </Alert>
</section>
