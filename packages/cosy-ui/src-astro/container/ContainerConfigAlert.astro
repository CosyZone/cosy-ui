---
import '../../style.ts';
import { Alert } from '../../index-astro';

interface IProps {
  messages: string[];
}

const { messages } = Astro.props as IProps;
---

<Alert type="warning" variant="soft" closable={false}>
  <ul class="cosy:list-disc cosy:pl-5 cosy:space-y-1">
    {messages.map((m) => <li>{m}</li>)}
  </ul>
</Alert>
