---
import { CodeContainer } from "@coffic/cosy-ui";
import PlaceHolderMutedSourceCode from "./PlaceHolderMuted.astro?raw";
import PlaceHolderNotMutedSourceCode from "./PlaceHolderNotMuted.astro?raw";
import PlaceHolderMuted from "./PlaceHolderMuted.astro";
import PlaceHolderNotMuted from "./PlaceHolderNotMuted.astro";
---

<CodeContainer
    codes={[PlaceHolderNotMutedSourceCode, PlaceHolderMutedSourceCode]}
    titles={['false', 'true']}>
    <div id="tab-1">
        <PlaceHolderNotMuted />
    </div>
    <div id="tab-2">
        <PlaceHolderMuted />
    </div>
</CodeContainer>
