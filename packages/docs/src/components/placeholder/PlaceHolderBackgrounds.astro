---
/**
 * @component PlaceHolderBackgrounds
 * @description PlaceHolder 组件背景色示例
 */
import { PlaceHolder, CodeContainer } from "@coffic/cosy-ui";
import PlaceHolderBackgroundBase100 from "./PlaceHolderBackgroundBase100.astro";
import PlaceHolderBackgroundBase200 from "./PlaceHolderBackgroundBase200.astro";
import PlaceHolderBackgroundBase300 from "./PlaceHolderBackgroundBase300.astro";
import PlaceHolderBackgroundPrimary from "./PlaceHolderBackgroundPrimary.astro";
import PlaceHolderBackgroundSecondary from "./PlaceHolderBackgroundSecondary.astro";
import PlaceHolderBackgroundAccent from "./PlaceHolderBackgroundAccent.astro";
import PlaceHolderBackgroundNeutral from "./PlaceHolderBackgroundNeutral.astro";
import PlaceHolderBackgroundInfo from "./PlaceHolderBackgroundInfo.astro";
import PlaceHolderBackgroundSuccess from "./PlaceHolderBackgroundSuccess.astro";
import PlaceHolderBackgroundWarning from "./PlaceHolderBackgroundWarning.astro";
import PlaceHolderBackgroundError from "./PlaceHolderBackgroundError.astro";
import PlaceHolderBackgroundBase100SourceCode from "./PlaceHolderBackgroundBase100.astro?raw";
import PlaceHolderBackgroundBase200SourceCode from "./PlaceHolderBackgroundBase200.astro?raw";
import PlaceHolderBackgroundBase300SourceCode from "./PlaceHolderBackgroundBase300.astro?raw";
import PlaceHolderBackgroundPrimarySourceCode from "./PlaceHolderBackgroundPrimary.astro?raw";
import PlaceHolderBackgroundSecondarySourceCode from "./PlaceHolderBackgroundSecondary.astro?raw";
import PlaceHolderBackgroundAccentSourceCode from "./PlaceHolderBackgroundAccent.astro?raw";
import PlaceHolderBackgroundNeutralSourceCode from "./PlaceHolderBackgroundNeutral.astro?raw";
import PlaceHolderBackgroundInfoSourceCode from "./PlaceHolderBackgroundInfo.astro?raw";
import PlaceHolderBackgroundSuccessSourceCode from "./PlaceHolderBackgroundSuccess.astro?raw";
import PlaceHolderBackgroundWarningSourceCode from "./PlaceHolderBackgroundWarning.astro?raw";
import PlaceHolderBackgroundErrorSourceCode from "./PlaceHolderBackgroundError.astro?raw";
---

<CodeContainer
  titles={[
    'base-100',
    'base-200',
    'base-300',
    'primary',
    'secondary',
    'accent',
    'neutral',
    'info',
    'success',
    'warning',
    'error',
  ]}
  codes={[
    PlaceHolderBackgroundBase100SourceCode,
    PlaceHolderBackgroundBase200SourceCode,
    PlaceHolderBackgroundBase300SourceCode,
    PlaceHolderBackgroundPrimarySourceCode,
    PlaceHolderBackgroundSecondarySourceCode,
    PlaceHolderBackgroundAccentSourceCode,
    PlaceHolderBackgroundNeutralSourceCode,
    PlaceHolderBackgroundInfoSourceCode,
    PlaceHolderBackgroundSuccessSourceCode,
    PlaceHolderBackgroundWarningSourceCode,
    PlaceHolderBackgroundErrorSourceCode,
  ]}>
  <div id="tab-1"><PlaceHolderBackgroundBase100 /></div>
  <div id="tab-2"><PlaceHolderBackgroundBase200 /></div>
  <div id="tab-3"><PlaceHolderBackgroundBase300 /></div>
  <div id="tab-4"><PlaceHolderBackgroundPrimary /></div>
  <div id="tab-5"><PlaceHolderBackgroundSecondary /></div>
  <div id="tab-6"><PlaceHolderBackgroundAccent /></div>
  <div id="tab-7"><PlaceHolderBackgroundNeutral /></div>
  <div id="tab-8"><PlaceHolderBackgroundInfo /></div>
  <div id="tab-9"><PlaceHolderBackgroundSuccess /></div>
  <div id="tab-10"><PlaceHolderBackgroundWarning /></div>
  <div id="tab-11"><PlaceHolderBackgroundError /></div>
</CodeContainer>
