---
/**
 * @component LinkVue FullWidth Container
 * @description LinkVue 全宽显示容器组件
 */

import { CodeContainer } from "@coffic/cosy-ui";
import LinkVueFullWidthNormal from "./LinkVueFullWidthNormal.vue";
import LinkVueFullWidthFull from "./LinkVueFullWidthFull.vue";
import LinkVueFullWidthNormalSource from "./LinkVueFullWidthNormal.vue?raw";
import LinkVueFullWidthFullSource from "./LinkVueFullWidthFull.vue?raw";
---

<CodeContainer
  titles={['普通宽度', '全宽显示']}
