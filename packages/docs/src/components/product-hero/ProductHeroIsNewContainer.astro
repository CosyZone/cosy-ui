import ProductHeroIsNewTrue from "./ProductHeroIsNewTrue.astro";
import ProductHeroIsNewFalse from "./ProductHeroIsNewFalse.astro";
import sourceTrue from "./ProductHeroIsNewTrue.astro?raw";
import sourceFalse from "./ProductHeroIsNewFalse.astro?raw";
