---
import { CodeContainer } from "@coffic/cosy-ui";
import ContactWithWebsite from "./ContactWithWebsite.astro";
import ContactWithoutWebsite from "./ContactWithoutWebsite.astro";
import ContactWithWebsiteSourceCode from "./ContactWithWebsite.astro?raw";
import ContactWithoutWebsiteSourceCode from "./ContactWithoutWebsite.astro?raw";
---

<CodeContainer
  titles={['有网站', '无网站']}
  codes={[ContactWithWebsiteSourceCode, ContactWithoutWebsiteSourceCode]}>
  <div id="tab-1">
    <ContactWithWebsite />
  </div>
  <div id="tab-2">
    <ContactWithoutWebsite />
  </div>
</CodeContainer>
