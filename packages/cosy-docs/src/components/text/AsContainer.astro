---



/**
 * @component TextAsContainer
 * @description Text as 属性容器，带源码展示。
 */
import { CodeContainer } from "@coffic/cosy-ui";
import AsDiv from "./as/AsDiv.astro";
import AsDivSourceCode from "./as/AsDiv.astro?raw";
import AsH1 from "./as/AsH1.astro";
import AsH1SourceCode from "./as/AsH1.astro?raw";
import AsLabel from "./as/AsLabel.astro";
import AsLabelSourceCode from "./as/AsLabel.astro?raw";
import AsSpan from "./as/AsSpan.astro";
import AsSpanSourceCode from "./as/AsSpan.astro?raw";
---

<CodeContainer
  titles={['span', 'div', 'h1', 'label']}
  codes={[
    AsSpanSourceCode,
    AsDivSourceCode,
    AsH1SourceCode,
    AsLabelSourceCode,
  ]}>
  <div id="tab-1"><AsSpan /></div>
  <div id="tab-2"><AsDiv /></div>
  <div id="tab-3"><AsH1 /></div>
  <div id="tab-4"><AsLabel /></div>
</CodeContainer>
