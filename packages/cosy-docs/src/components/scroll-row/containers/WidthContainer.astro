---
import { CodeContainer } from '@coffic/cosy-ui';
import WidthNoneSourceCode from '../WidthNone.astro?raw';
import Width3xsSourceCode from '../Width3xs.astro?raw';
import Width2xsSourceCode from '../Width2xs.astro?raw';
import WidthXsSourceCode from '../WidthXs.astro?raw';
import WidthSmSourceCode from '../WidthSm.astro?raw';
import WidthMdSourceCode from '../WidthMd.astro?raw';
import WidthLgSourceCode from '../WidthLg.astro?raw';
import WidthXlSourceCode from '../WidthXl.astro?raw';
import Width2xlSourceCode from '../Width2xl.astro?raw';
import Width3xlSourceCode from '../Width3xl.astro?raw';
import Width4xlSourceCode from '../Width4xl.astro?raw';
import Width5xlSourceCode from '../Width5xl.astro?raw';
import Width6xlSourceCode from '../Width6xl.astro?raw';
import WidthFullSourceCode from '../WidthFull.astro?raw';

import WidthNone from '../WidthNone.astro';
import Width3xs from '../Width3xs.astro';
import Width2xs from '../Width2xs.astro';
import WidthXs from '../WidthXs.astro';
import WidthSm from '../WidthSm.astro';
import WidthMd from '../WidthMd.astro';
import WidthLg from '../WidthLg.astro';
import WidthXl from '../WidthXl.astro';
import Width2xl from '../Width2xl.astro';
import Width3xl from '../Width3xl.astro';
import Width4xl from '../Width4xl.astro';
import Width5xl from '../Width5xl.astro';
import Width6xl from '../Width6xl.astro';
import WidthFull from '../WidthFull.astro';
---

<CodeContainer
  codes={[
    WidthNoneSourceCode,
    Width3xsSourceCode,
    Width2xsSourceCode,
    WidthXsSourceCode,
    WidthSmSourceCode,
    WidthMdSourceCode,
    WidthLgSourceCode,
    WidthXlSourceCode,
    Width2xlSourceCode,
    Width3xlSourceCode,
    Width4xlSourceCode,
    Width5xlSourceCode,
    Width6xlSourceCode,
    WidthFullSourceCode,
  ]}
  titles={[
    'none',
    '3xs',
    '2xs',
    'xs',
    'sm',
    'md',
    'lg',
    'xl',
    '2xl',
    '3xl',
    '4xl',
    '5xl',
    '6xl',
    'full',
  ]}>
  <div id="tab-1">
    <WidthNone />
  </div>
  <div id="tab-2">
    <Width3xs />
  </div>
  <div id="tab-3">
    <Width2xs />
  </div>
  <div id="tab-4">
    <WidthXs />
  </div>
  <div id="tab-5">
    <WidthSm />
  </div>
  <div id="tab-6">
    <WidthMd />
  </div>
  <div id="tab-7">
    <WidthLg />
  </div>
  <div id="tab-8">
    <WidthXl />
  </div>
  <div id="tab-9">
    <Width2xl />
  </div>
  <div id="tab-10">
    <Width3xl />
  </div>
  <div id="tab-11">
    <Width4xl />
  </div>
  <div id="tab-12">
    <Width5xl />
  </div>
  <div id="tab-13">
    <Width6xl />
  </div>
  <div id="tab-14">
    <WidthFull />
  </div>
</CodeContainer>
