---

import { CodeContainer } from "@coffic/cosy-ui";
import GapLg from "../GapLg.astro";
import GapLgSource from "../GapLg.astro?raw";
import GapMd from "../GapMd.astro";
import GapMdSource from "../GapMd.astro?raw";
import GapNone from "../GapNone.astro";
import GapNoneSource from "../GapNone.astro?raw";
import GapSm from "../GapSm.astro";
import GapSmSource from "../GapSm.astro?raw";
import GapXl from "../GapXl.astro";
import GapXlSource from "../GapXl.astro?raw";
import GapXs from "../GapXs.astro";
import GapXsSource from "../GapXs.astro?raw";
---

<CodeContainer
  codes={[
    GapNoneSource,
    GapXsSource,
    GapSmSource,
    GapMdSource,
    GapLgSource,
    GapXlSource,
  ]}
  titles={['none', 'xs', 'sm', 'md', 'lg', 'xl']}>
  <div id="tab-1">
    <GapNone />
  </div>
  <div id="tab-2">
    <GapXs />
  </div>
  <div id="tab-3">
    <GapSm />
  </div>
  <div id="tab-4">
    <GapMd />
  </div>
  <div id="tab-5">
    <GapLg />
  </div>
  <div id="tab-6">
    <GapXl />
  </div>
</CodeContainer>
