---
/**
 * @component CodePanelLanguageSupportContainer
 * @description CodePanel 组件多语言支持展示容器，带源码展示，支持多标签切换。
 */
import { CodeContainer } from "@coffic/cosy-ui";
import LanguageAstro from "./LanguageAstro.astro";
import LanguageGo from "./LanguageGo.astro";
import LanguageRust from "./LanguageRust.astro";
import LanguageSQL from "./LanguageSQL.astro";
import LanguageAstroSource from "./LanguageAstro.astro?raw";
import LanguageGoSource from "./LanguageGo.astro?raw";
import LanguageRustSource from "./LanguageRust.astro?raw";
import LanguageSQLSource from "./LanguageSQL.astro?raw";

const codes = [
	LanguageAstroSource,
	LanguageGoSource,
	LanguageRustSource,
	LanguageSQLSource,
];
const titles = ["Astro", "Go", "Rust", "SQL"];
---

<CodeContainer codes={codes} titles={titles}>
  <div id="tab-1">
    <LanguageAstro />
  </div>
  <div id="tab-2">
    <LanguageGo />
  </div>
  <div id="tab-3">
    <LanguageRust />
  </div>
  <div id="tab-4">
    <LanguageSQL />
  </div>
</CodeContainer>
