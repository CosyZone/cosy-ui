---



import { CodeContainer } from "@coffic/cosy-ui";
import Lg from "./shadow/Lg.astro";
import LgSourceCode from "./shadow/Lg.astro?raw";
import Md from "./shadow/Md.astro";
import MdSourceCode from "./shadow/Md.astro?raw";
import None from "./shadow/None.astro";
import NoneSourceCode from "./shadow/None.astro?raw";
import Sm from "./shadow/Sm.astro";
import SmSourceCode from "./shadow/Sm.astro?raw";
import TwoXl from "./shadow/TwoXl.astro";
import TwoXlSourceCode from "./shadow/TwoXl.astro?raw";
import Xl from "./shadow/Xl.astro";
import XlSourceCode from "./shadow/Xl.astro?raw";
---

<CodeContainer
  codes={[
    NoneSourceCode,
    SmSourceCode,
    MdSourceCode,
    LgSourceCode,
    XlSourceCode,
    TwoXlSourceCode,
  ]}
  titles={['none', 'sm', 'md', 'lg', 'xl', '2xl']}>
  <div id="tab-1">
    <None />
  </div>
  <div id="tab-2">
    <Sm />
  </div>
  <div id="tab-3">
    <Md />
  </div>
  <div id="tab-4">
    <Lg />
  </div>
  <div id="tab-5">
    <Xl />
  </div>
  <div id="tab-6">
    <TwoXl />
  </div>
</CodeContainer>
