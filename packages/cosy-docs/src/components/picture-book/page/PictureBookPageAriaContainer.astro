---
import { CodeContainer } from '@coffic/cosy-ui';
import AriaDefault from './PictureBookPageAriaDefault.astro';
import AriaCustom from './PictureBookPageAriaCustom.astro';

import AriaDefaultCode from './PictureBookPageAriaDefault.astro?raw';
import AriaCustomCode from './PictureBookPageAriaCustom.astro?raw';
---

<CodeContainer
  codes={[AriaDefaultCode, AriaCustomCode]}
  titles={['default', 'custom']}>
  <div id="tab-1" style="width: 30%;margin: 0 auto">
    <AriaDefault />
  </div>
  <div id="tab-2" style="width: 30%;margin: 0 auto">
    <AriaCustom />
  </div>
</CodeContainer>
