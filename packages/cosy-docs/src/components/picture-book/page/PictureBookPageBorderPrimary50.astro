---

import { PictureBookPage, PictureBookTextBox } from "@coffic/cosy-ui";
import PictureBookBg3 from "@/assets/picture-book-bg-3.svg";
---

<PictureBookPage
    background={PictureBookBg3}
    pageAspectRatio={3 / 4}
    border="primary/50">
    <PictureBookTextBox
        slot="overlay"
        top={24}
        left={10}
        width={85}
        bg="primary"
        paddingPx={12}
        roundedPx={10}>
        自定义边框色 (primary/50)
    </PictureBookTextBox>
</PictureBookPage>
