---



import { PictureBookPage, PictureBookTextBox } from "@coffic/cosy-ui";
import SampleImage from "@/assets/hero.png";
---

<PictureBookPage background={SampleImage} pageAspectRatio={3 / 4}>
    <PictureBookTextBox
        slot="overlay"
        top={20}
        left={10}
        width={80}
        bg="primary"
        paddingPx={12}
        roundedPx={12}>
        background={SampleImage}
    </PictureBookTextBox>
</PictureBookPage>
