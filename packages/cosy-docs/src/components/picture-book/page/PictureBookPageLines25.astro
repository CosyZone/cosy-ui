---
import { PictureBookPage, PictureBookTextBox } from '@coffic/cosy-ui';
---

<PictureBookPage lines={25} ariaLabelPrefix="绘本页(25行)">
  <div slot="background" style="width:100%;height:100%;background:#f5f5f5">
  </div>
  <PictureBookTextBox
    slot="overlay"
    top={10}
    left={8}
    width={84}
    heightInLines={4}
    bgColor="#ffffffcc"
    paddingPx={10}
    roundedPx={8}>
    25 行模式：行距更大。
  </PictureBookTextBox>
</PictureBookPage>
