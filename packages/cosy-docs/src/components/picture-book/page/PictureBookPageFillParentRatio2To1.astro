---
import {
  Container,
  PictureBookPage,
  PictureBookTextBox,
} from '@coffic/cosy-ui';
---

<Container
  background="secondary/20"
  rounded="md"
  padding="md"
  width="sm"
  height="lg">
  <PictureBookPage
    fillParent={true}
    pageAspectRatio={2 / 1}
    background="primary/20">
    <PictureBookTextBox slot="overlay" top={10} left={20} width={60}>
      2:1 宽高比：宽扁比例
    </PictureBookTextBox>
  </PictureBookPage>
</Container>
