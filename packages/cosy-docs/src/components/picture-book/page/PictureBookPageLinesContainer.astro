---
import { CodeContainer } from '@coffic/cosy-ui';
import Lines25 from './PictureBookPageLines25.astro';
import Lines45 from './PictureBookPageLines45.astro';

import Lines25Code from './PictureBookPageLines25.astro?raw';
import Lines45Code from './PictureBookPageLines45.astro?raw';
---

<CodeContainer codes={[Lines25Code, Lines45Code]} titles={['25', '45']}>
  <div id="tab-1" style="width: 50%;margin: 0 auto">
    <Lines25 />
  </div>
  <div id="tab-2" style="width: 50%;margin: 0 auto">
    <Lines45 />
  </div>
</CodeContainer>
