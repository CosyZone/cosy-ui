---
import { PictureBookPage, PictureBookTextBox } from "@coffic/cosy-ui";
import PictureBookBg1 from "@/assets/picture-book-bg.svg";
---

<PictureBookPage
  background={PictureBookBg1}
  pageAspectRatio={3 / 4}
  border={true}>
  <PictureBookTextBox
    slot="overlay"
    top={12}
    left={10}
    width={80}
    bg="paper"
    paddingPx={12}
    roundedPx={10}>
    默认边框 (true)
  </PictureBookTextBox>
</PictureBookPage>
