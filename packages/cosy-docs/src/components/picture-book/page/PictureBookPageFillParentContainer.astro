---
import { CodeContainer } from '@coffic/cosy-ui';
import DefaultBehavior from './PictureBookPageDefault.astro';
import FillParent from './PictureBookPageFillParent.astro';

import DefaultBehaviorCode from './PictureBookPageDefault.astro?raw';
import FillParentCode from './PictureBookPageFillParent.astro?raw';
---

<CodeContainer
  codes={[DefaultBehaviorCode, FillParentCode]}
  titles={['默认行为', '占满父容器']}>
  <div id="tab-1" style="width: 100%; margin: 0 auto">
    <DefaultBehavior />
  </div>
  <div id="tab-2" style="width: 100%; margin: 0 auto">
    <FillParent />
  </div>
</CodeContainer>
