---
import { CodeContainer } from "@coffic/cosy-ui";
import LinesOn from "./PictureBookPageLinesOn.astro";
import LinesOff from "./PictureBookPageLinesOff.astro";

import LinesOnCode from "./PictureBookPageLinesOn.astro?raw";
import LinesOffCode from "./PictureBookPageLinesOff.astro?raw";
---

<CodeContainer codes={[LinesOnCode, LinesOffCode]} titles={['on', 'off']}>
  <div id="tab-1" style="width: 30%;margin: 0 auto">
    <LinesOn />
  </div>
  <div id="tab-2" style="width: 30%;margin: 0 auto">
    <LinesOff />
  </div>
</CodeContainer>
