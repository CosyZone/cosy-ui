---



import { CodeContainer } from "@coffic/cosy-ui";
import Long from "./DefaultTextLong.astro";
import LongCode from "./DefaultTextLong.astro?raw";
import Simple from "./DefaultTextSimple.astro";
import SimpleCode from "./DefaultTextSimple.astro?raw";
---

<CodeContainer codes={[SimpleCode, LongCode]} titles={['simple', 'long']}>
  <div id="tab-1" style="width: 30%;margin: 0 auto">
    <Simple />
  </div>
  <div id="tab-2" style="width: 30%;margin: 0 auto">
    <Long />
  </div>
</CodeContainer>
