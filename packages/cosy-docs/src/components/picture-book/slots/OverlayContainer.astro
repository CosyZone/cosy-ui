---
import { CodeContainer } from '@coffic/cosy-ui';
import One from './OverlaySingleBox.astro';
import Two from './OverlayTwoBoxes.astro';

import OneCode from './OverlaySingleBox.astro?raw';
import TwoCode from './OverlayTwoBoxes.astro?raw';
---

<CodeContainer codes={[OneCode, TwoCode]} titles={['single', 'two']}>
  <div id="tab-1">
    <One />
  </div>
  <div id="tab-2">
    <Two />
  </div>
</CodeContainer>
