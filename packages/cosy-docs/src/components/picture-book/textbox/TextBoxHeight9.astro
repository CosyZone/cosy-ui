---
import { PictureBookPage, PictureBookTextBox } from '@coffic/cosy-ui';
---

<PictureBookPage lines={35} ariaLabelPrefix="高度9行">
  <div slot="background" style="width:100%;height:100%;background:#fff"></div>
  <PictureBookTextBox
    slot="overlay"
    top={18}
    left={10}
    width={80}
    heightInLines={9}
    bgColor="#ffffffcc"
    paddingPx={10}
    roundedPx={8}>
    高度 9 行
  </PictureBookTextBox>
</PictureBookPage>
