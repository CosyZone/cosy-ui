---

import { CodeContainer } from "@coffic/cosy-ui";
import RoundedCqw2 from "./TextBoxRoundedCqw2.astro";
import RoundedCqw2Code from "./TextBoxRoundedCqw2.astro?raw";
import RoundedCqw4 from "./TextBoxRoundedCqw4.astro";
import RoundedCqw4Code from "./TextBoxRoundedCqw4.astro?raw";
---

<CodeContainer
  codes={[RoundedCqw2Code, RoundedCqw4Code]}
  titles={['2cqw', '4cqw']}>
  <div id="tab-1" style="width: 30%;margin: 0 auto">
    <RoundedCqw2 />
  </div>
  <div id="tab-2" style="width: 30%;margin: 0 auto">
    <RoundedCqw4 />
  </div>
</CodeContainer>
