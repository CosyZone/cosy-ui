---
import { PictureBookPage, PictureBookTextBox } from '@coffic/cosy-ui';
---

<PictureBookPage ariaLabelPrefix="文本阴影开">
  <div slot="background" style="width:100%;height:100%;background:#e8f0fe">
  </div>
  <PictureBookTextBox
    slot="overlay"
    top={18}
    left={10}
    width={80}
    bgColor="#ffffffbb"
    paddingPx={12}
    roundedPx={12}
    textShadow>
    文本阴影：开启
  </PictureBookTextBox>
</PictureBookPage>
