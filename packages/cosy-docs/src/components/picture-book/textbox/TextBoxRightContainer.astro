---
import { CodeContainer } from '@coffic/cosy-ui';
import R10 from './TextBoxRight10.astro';
import R30 from './TextBoxRight30.astro';

import R10Code from './TextBoxRight10.astro?raw';
import R30Code from './TextBoxRight30.astro?raw';
---

<CodeContainer codes={[R10Code, R30Code]} titles={['10%', '30%']}>
  <div id="tab-1" style="width: 50%;margin: 0 auto">
    <R10 />
  </div>
  <div id="tab-2" style="width: 50%;margin: 0 auto">
    <R30 />
  </div>
</CodeContainer>
