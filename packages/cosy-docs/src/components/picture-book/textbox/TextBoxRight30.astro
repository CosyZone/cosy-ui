---
import { PictureBookPage, PictureBookTextBox } from '@coffic/cosy-ui';
---

<PictureBookPage ariaLabelPrefix="right 30%">
  <div slot="background" style="width:100%;height:100%;background:#fff"></div>
  <PictureBookTextBox
    slot="overlay"
    top={20}
    right={30}
    width={60}
    bgColor="#ffffffcc"
    paddingPx={10}
    roundedPx={8}>
    right = 30%
  </PictureBookTextBox>
</PictureBookPage>
