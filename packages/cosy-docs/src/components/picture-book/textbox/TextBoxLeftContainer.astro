---
import { CodeContainer } from '@coffic/cosy-ui';
import L10 from './TextBoxLeft10.astro';
import L30 from './TextBoxLeft30.astro';

import L10Code from './TextBoxLeft10.astro?raw';
import L30Code from './TextBoxLeft30.astro?raw';
---

<CodeContainer codes={[L10Code, L30Code]} titles={['10%', '30%']}>
  <div id="tab-1">
    <L10 />
  </div>
  <div id="tab-2">
    <L30 />
  </div>
</CodeContainer>
