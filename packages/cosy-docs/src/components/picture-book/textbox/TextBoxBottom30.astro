---
import { PictureBookPage, PictureBookTextBox } from '@coffic/cosy-ui';
---

<PictureBookPage ariaLabelPrefix="bottom 30%">
  <div slot="background" style="width:100%;height:100%;background:#fff"></div>
  <PictureBookTextBox
    slot="overlay"
    bottom={30}
    left={10}
    width={80}
    bg="paper"
    paddingPx={10}
    roundedPx={8}>
    bottom = 30%
  </PictureBookTextBox>
</PictureBookPage>
