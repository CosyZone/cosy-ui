---
/**
 * @component ApplePhoneBackgroundColorContainer
 * @description 展示 ApplePhone 组件的 backgroundColor 属性多种取值，Astro/Vue 各自独立标签展示。
 */
import { CodeContainer } from '@coffic/cosy-ui';
import ApplePhoneBackgroundColorPrimary from './ApplePhoneBackgroundColorPrimary.astro';
import ApplePhoneBackgroundColorPrimaryRaw from './ApplePhoneBackgroundColorPrimary.astro?raw';
import ApplePhoneBackgroundColorPrimaryVue from './ApplePhoneBackgroundColorPrimaryVue.vue';
import ApplePhoneBackgroundColorPrimaryVueRaw from './ApplePhoneBackgroundColorPrimaryVue.vue?raw';
import ApplePhoneBackgroundColorSuccess from './ApplePhoneBackgroundColorSuccess.astro';
import ApplePhoneBackgroundColorSuccessRaw from './ApplePhoneBackgroundColorSuccess.astro?raw';
import ApplePhoneBackgroundColorSuccessVue from './ApplePhoneBackgroundColorSuccessVue.vue';
import ApplePhoneBackgroundColorSuccessVueRaw from './ApplePhoneBackgroundColorSuccessVue.vue?raw';
import ApplePhoneBackgroundColorWarning from './ApplePhoneBackgroundColorWarning.astro';
import ApplePhoneBackgroundColorWarningRaw from './ApplePhoneBackgroundColorWarning.astro?raw';
import ApplePhoneBackgroundColorWarningVue from './ApplePhoneBackgroundColorWarningVue.vue';
import ApplePhoneBackgroundColorWarningVueRaw from './ApplePhoneBackgroundColorWarningVue.vue?raw';
---

<CodeContainer
	codes={[
		ApplePhoneBackgroundColorPrimaryRaw,
		ApplePhoneBackgroundColorSuccessRaw,
		ApplePhoneBackgroundColorWarningRaw,
		ApplePhoneBackgroundColorPrimaryVueRaw,
		ApplePhoneBackgroundColorSuccessVueRaw,
		ApplePhoneBackgroundColorWarningVueRaw,
	]}
	titles={[
		'primary/10',
		'success/10',
		'warning/10',
		'primary/10',
		'success/10',
		'warning/10',
	]}
>
	<div id="tab-1" data-label="astro"><ApplePhoneBackgroundColorPrimary /></div>
	<div id="tab-2" data-label="astro"><ApplePhoneBackgroundColorSuccess /></div>
	<div id="tab-3" data-label="astro"><ApplePhoneBackgroundColorWarning /></div>
	<div id="tab-4" data-label="vue"><ApplePhoneBackgroundColorPrimaryVue /></div>
	<div id="tab-5" data-label="vue"><ApplePhoneBackgroundColorSuccessVue /></div>
	<div id="tab-6" data-label="vue"><ApplePhoneBackgroundColorWarningVue /></div>
</CodeContainer>
