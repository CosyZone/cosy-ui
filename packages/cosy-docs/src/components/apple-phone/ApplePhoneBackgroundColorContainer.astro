---

import { CodeContainer } from "@coffic/cosy-ui";
import ApplePhoneBackgroundColorPrimary from "./ApplePhoneBackgroundColorPrimary.astro";
import ApplePhoneBackgroundColorPrimarySourceCode from "./ApplePhoneBackgroundColorPrimary.astro?raw";
import ApplePhoneBackgroundColorSuccess from "./ApplePhoneBackgroundColorSuccess.astro";
import ApplePhoneBackgroundColorSuccessSourceCode from "./ApplePhoneBackgroundColorSuccess.astro?raw";
import ApplePhoneBackgroundColorWarning from "./ApplePhoneBackgroundColorWarning.astro";
import ApplePhoneBackgroundColorWarningSourceCode from "./ApplePhoneBackgroundColorWarning.astro?raw";
---

<CodeContainer
    codes={[
        ApplePhoneBackgroundColorPrimarySourceCode,
        ApplePhoneBackgroundColorSuccessSourceCode,
        ApplePhoneBackgroundColorWarningSourceCode,
    ]}
    titles={['primary/10', 'success/10', 'warning/10']}>
    <div id="tab-1">
        <ApplePhoneBackgroundColorPrimary />
    </div>
    <div id="tab-2">
        <ApplePhoneBackgroundColorSuccess />
    </div>
    <div id="tab-3">
        <ApplePhoneBackgroundColorWarning />
    </div>
</CodeContainer>
