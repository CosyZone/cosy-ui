---
import { CodeContainer } from "@coffic/cosy-ui";
import ApplePhoneHeightSmSourceCode from "./ApplePhoneHeightSm.astro?raw";
import ApplePhoneHeightMdSourceCode from "./ApplePhoneHeightMd.astro?raw";
import ApplePhoneHeightLgSourceCode from "./ApplePhoneHeightLg.astro?raw";
import ApplePhoneHeightXlSourceCode from "./ApplePhoneHeightXl.astro?raw";
import ApplePhoneHeight2xlSourceCode from "./ApplePhoneHeight2xl.astro?raw";
import ApplePhoneHeightSm from "./ApplePhoneHeightSm.astro";
import ApplePhoneHeightMd from "./ApplePhoneHeightMd.astro";
import ApplePhoneHeightLg from "./ApplePhoneHeightLg.astro";
import ApplePhoneHeightXl from "./ApplePhoneHeightXl.astro";
import ApplePhoneHeight2xl from "./ApplePhoneHeight2xl.astro";
---

<CodeContainer
    codes={[
        ApplePhoneHeightSmSourceCode,
        ApplePhoneHeightMdSourceCode,
        ApplePhoneHeightLgSourceCode,
        ApplePhoneHeightXlSourceCode,
        ApplePhoneHeight2xlSourceCode,
    ]}
    titles={['sm', 'md', 'lg', 'xl', '2xl']}>
    <div id="tab-1">
        <ApplePhoneHeightSm />
    </div>
    <div id="tab-2">
        <ApplePhoneHeightMd />
    </div>
    <div id="tab-3">
        <ApplePhoneHeightLg />
    </div>
    <div id="tab-4">
        <ApplePhoneHeightXl />
    </div>
    <div id="tab-5">
        <ApplePhoneHeight2xl />
    </div>
</CodeContainer>
