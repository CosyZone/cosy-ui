---
import { CodeContainer } from "@coffic/cosy-ui";
import BannerBoxVueBackgroundsSourceCode from "./BannerBoxVueBackgrounds.vue?raw";
import BannerBoxVueBackgroundSecondarySourceCode from "./BannerBoxVueBackgroundSecondary.vue?raw";
import BannerBoxVueBackgroundOpacitySourceCode from "./BannerBoxVueBackgroundOpacity.vue?raw";
import BannerBoxVueBackgroundGradientSourceCode from "./BannerBoxVueBackgroundGradient.vue?raw";
import BannerBoxVueBackgrounds from "./BannerBoxVueBackgrounds.vue";
import BannerBoxVueBackgroundSecondary from "./BannerBoxVueBackgroundSecondary.vue";
import BannerBoxVueBackgroundOpacity from "./BannerBoxVueBackgroundOpacity.vue";
import BannerBoxVueBackgroundGradient from "./BannerBoxVueBackgroundGradient.vue";
---

<CodeContainer
  codes={[
    BannerBoxVueBackgroundsSourceCode,
    BannerBoxVueBackgroundSecondarySourceCode,
    BannerBoxVueBackgroundOpacitySourceCode,
    BannerBoxVueBackgroundGradientSourceCode,
  ]}
  titles={['primary', 'secondary', 'primary/50', 'gradient-sky']}>
  <div id="tab-1">
    <BannerBoxVueBackgrounds />
  </div>
  <div id="tab-2">
    <BannerBoxVueBackgroundSecondary />
  </div>
  <div id="tab-3">
    <BannerBoxVueBackgroundOpacity />
  </div>
  <div id="tab-4">
    <BannerBoxVueBackgroundGradient />
  </div>
</CodeContainer>
