---
/**
 * @component TeamMemberRoundedContainer
 * @description TeamMember rounded 多标签演示容器，展示源码和效果。
 */
import { CodeContainer } from '@coffic/cosy-ui';
import TeamMemberRoundedNone from './TeamMemberRoundedNone.astro';
import TeamMemberRoundedNoneSource from './TeamMemberRoundedNone.astro?raw';
import TeamMemberRoundedSm from './TeamMemberRoundedSm.astro';
import TeamMemberRoundedSmSource from './TeamMemberRoundedSm.astro?raw';
import TeamMemberRoundedLg from './TeamMemberRoundedLg.astro';
import TeamMemberRoundedLgSource from './TeamMemberRoundedLg.astro?raw';
import TeamMemberRoundedXl from './TeamMemberRoundedXl.astro';
import TeamMemberRoundedXlSource from './TeamMemberRoundedXl.astro?raw';
---

<CodeContainer
    codes={[
        TeamMemberRoundedNoneSource,
        TeamMemberRoundedSmSource,
        TeamMemberRoundedLgSource,
        TeamMemberRoundedXlSource,
    ]}
    titles={['none', 'sm', 'lg', 'xl']}>
    <div id="tab-1" class="cosy:max-w-xs cosy:mx-auto">
        <TeamMemberRoundedNone />
    </div>
    <div id="tab-2" class="cosy:max-w-xs cosy:mx-auto">
        <TeamMemberRoundedSm />
    </div>
    <div id="tab-3" class="cosy:max-w-xs cosy:mx-auto">
        <TeamMemberRoundedLg />
    </div>
    <div id="tab-4" class="cosy:max-w-xs cosy:mx-auto">
        <TeamMemberRoundedXl />
    </div>
</CodeContainer>
