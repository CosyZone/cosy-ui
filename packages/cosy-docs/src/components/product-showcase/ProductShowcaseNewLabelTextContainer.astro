---
import { CodeContainer } from '@coffic/cosy-ui';
import ProductShowcaseNewLabelTextDefault from './ProductShowcaseNewLabelTextDefault.astro';
import ProductShowcaseNewLabelTextCustom from './ProductShowcaseNewLabelTextCustom.astro';
import ProductShowcaseNewLabelTextEnglish from './ProductShowcaseNewLabelTextEnglish.astro';
import sourceDefault from './ProductShowcaseNewLabelTextDefault.astro?raw';
import sourceCustom from './ProductShowcaseNewLabelTextCustom.astro?raw';
import sourceEnglish from './ProductShowcaseNewLabelTextEnglish.astro?raw';
---

<CodeContainer
  codes={[sourceDefault, sourceCustom, sourceEnglish]}
  titles={['默认', '自定义', '英文']}>
  <div id="tab-1">
    <ProductShowcaseNewLabelTextDefault />
  </div>
  <div id="tab-2">
    <ProductShowcaseNewLabelTextCustom />
  </div>
  <div id="tab-3">
    <ProductShowcaseNewLabelTextEnglish />
  </div>
</CodeContainer>
