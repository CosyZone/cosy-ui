---
import { CodeContainer } from '@coffic/cosy-ui';
import ProductShowcaseLearnMoreTextDefault from './ProductShowcaseLearnMoreTextDefault.astro';
import ProductShowcaseLearnMoreTextCustom from './ProductShowcaseLearnMoreTextCustom.astro';
import ProductShowcaseLearnMoreTextEnglish from './ProductShowcaseLearnMoreTextEnglish.astro';
import sourceDefault from './ProductShowcaseLearnMoreTextDefault.astro?raw';
import sourceCustom from './ProductShowcaseLearnMoreTextCustom.astro?raw';
import sourceEnglish from './ProductShowcaseLearnMoreTextEnglish.astro?raw';
---

<CodeContainer
  codes={[sourceDefault, sourceCustom, sourceEnglish]}
  titles={['默认', '自定义', '英文']}>
  <div id="tab-1">
    <ProductShowcaseLearnMoreTextDefault />
  </div>
  <div id="tab-2">
    <ProductShowcaseLearnMoreTextCustom />
  </div>
  <div id="tab-3">
    <ProductShowcaseLearnMoreTextEnglish />
  </div>
</CodeContainer>
