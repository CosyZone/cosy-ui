---
/**
 * @component ImageRowNavigationContainer
 * @description ImageRow 组件左右导航功能示例容器
 */
import { CodeContainer } from "@coffic/cosy-ui";
import ImageRowNavigationDisabled from "./ImageRowNavigationDisabled.astro";
import ImageRowNavigationEnabled from "./ImageRowNavigationEnabled.astro";
import ImageRowNavigationDisabledSourceCode from "./ImageRowNavigationDisabled.astro?raw";
import ImageRowNavigationEnabledSourceCode from "./ImageRowNavigationEnabled.astro?raw";
---

<CodeContainer
  titles={['禁用导航', '启用导航']}
  codes={[
    ImageRowNavigationDisabledSourceCode,
    ImageRowNavigationEnabledSourceCode,
  ]}>
  <div id="tab-1"><ImageRowNavigationDisabled /></div>
  <div id="tab-2"><ImageRowNavigationEnabled /></div>
</CodeContainer>
