---



/**
 * @component SocialIconColorsContainer
 * @description SocialIcon 组件 color 属性示例容器，展示不同颜色的社交图标。
 */
import { CodeContainer } from "@coffic/cosy-ui";
import ColorAccent from "./ColorAccent.vue";
import ColorAccentSource from "./ColorAccent.vue?raw";
import ColorNeutral from "./ColorNeutral.vue";
import ColorNeutralSource from "./ColorNeutral.vue?raw";
import ColorPrimary from "./ColorPrimary.vue";
import ColorPrimarySource from "./ColorPrimary.vue?raw";
import ColorSecondary from "./ColorSecondary.vue";
import ColorSecondarySource from "./ColorSecondary.vue?raw";
---

<CodeContainer
  codes={[
    ColorPrimarySource,
    ColorSecondarySource,
    ColorAccentSource,
    ColorNeutralSource,
  ]}
  titles={['主要颜色', '次要颜色', '强调颜色', '中性颜色']}>
  <div id="tab-1">
    <ColorPrimary client:load />
  </div>
  <div id="tab-2">
    <ColorSecondary client:load />
  </div>
  <div id="tab-3">
    <ColorAccent client:load />
  </div>
  <div id="tab-4">
    <ColorNeutral client:load />
  </div>
</CodeContainer>
