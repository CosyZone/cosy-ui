---



/**
 * @component HeadingMarginContainer
 * @description Heading 组件 margin 属性示例容器，展示不同的外边距。
 */
import { CodeContainer } from "@coffic/cosy-ui";
import MarginLarge from "./MarginLarge.vue";
import MarginLargeSource from "./MarginLarge.vue?raw";
import MarginMedium from "./MarginMedium.vue";
import MarginMediumSource from "./MarginMedium.vue?raw";
import MarginNone from "./MarginNone.vue";
import MarginNoneSource from "./MarginNone.vue?raw";
import MarginSmall from "./MarginSmall.vue";
import MarginSmallSource from "./MarginSmall.vue?raw";
import MarginXLarge from "./MarginXLarge.vue";
import MarginXLargeSource from "./MarginXLarge.vue?raw";
---

<CodeContainer
  codes={[
    MarginNoneSource,
    MarginSmallSource,
    MarginMediumSource,
    MarginLargeSource,
    MarginXLargeSource,
  ]}
  titles={['无外边距', '小外边距', '中等外边距', '大外边距', '超大外边距']}>
  <div id="tab-1">
    <MarginNone client:load />
  </div>
  <div id="tab-2">
    <MarginSmall client:load />
  </div>
  <div id="tab-3">
    <MarginMedium client:load />
  </div>
  <div id="tab-4">
    <MarginLarge client:load />
  </div>
  <div id="tab-5">
    <MarginXLarge client:load />
  </div>
</CodeContainer>
