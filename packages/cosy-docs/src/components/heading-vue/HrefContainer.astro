---



/**
 * @component HeadingHrefContainer
 * @description Heading 组件 href 属性示例容器，展示链接标题功能。
 */
import { CodeContainer } from "@coffic/cosy-ui";
import HrefAbout from "./HrefAbout.vue";
import HrefAboutSource from "./HrefAbout.vue?raw";
import HrefContact from "./HrefContact.vue";
import HrefContactSource from "./HrefContact.vue?raw";
import HrefProducts from "./HrefProducts.vue";
import HrefProductsSource from "./HrefProducts.vue?raw";
---

<CodeContainer
  codes={[HrefAboutSource, HrefContactSource, HrefProductsSource]}
  titles={['关于我们', '联系我们', '产品介绍']}>
  <div id="tab-1">
    <HrefAbout client:load />
  </div>
  <div id="tab-2">
    <HrefContact client:load />
  </div>
  <div id="tab-3">
    <HrefProducts client:load />
  </div>
</CodeContainer>
