---

/**
 * @component AlertWithTitleContainer
 * @description title 属性为空和有值两种情况，Astro/Vue 各两组。
 */
import { CodeContainer } from '@coffic/cosy-ui';
import AlertWithoutTitle from './AlertWithoutTitle.astro';
import AlertWithoutTitleRaw from './AlertWithoutTitle.astro?raw';
import AlertWithoutTitleVue from './AlertWithoutTitleVue.vue';
import AlertWithoutTitleVueRaw from './AlertWithoutTitleVue.vue?raw';
import AlertWithTitle from './AlertWithTitle.astro';
import AlertWithTitleRaw from './AlertWithTitle.astro?raw';
import AlertWithTitleVue from './AlertWithTitleVue.vue';
import AlertWithTitleVueRaw from './AlertWithTitleVue.vue?raw';
---
<CodeContainer
    codes={[
        AlertWithTitleRaw,
        AlertWithoutTitleRaw,
        AlertWithTitleVueRaw,
        AlertWithoutTitleVueRaw
    ]}
    titles={[
        '有标题',
        '无标题',
        '有标题',
        '无标题'
    ]}
>
    <div id="tab-1" data-label="astro"><AlertWithTitle /></div>
    <div id="tab-2" data-label="astro"><AlertWithoutTitle /></div>
    <div id="tab-3" data-label="vue"><AlertWithTitleVue /></div>
    <div id="tab-4" data-label="vue"><AlertWithoutTitleVue /></div>
</CodeContainer>
