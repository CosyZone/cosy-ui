        'xs',
        'sm',
        'md',
        'lg',
        'xl',
        'xs Factory',
        'sm Factory',
        'md Factory',
        'lg Factory',
        'xl Factory',
    ]}
    codes={[
        AlertMarginYXsSource,
        AlertMarginYSmSource,
        AlertMarginYMdSource,
        AlertMarginYLgSource,
        AlertMarginYXlSource,
        AlertMarginYXsFactorySource,
        AlertMarginYSmFactorySource,
        AlertMarginYMdFactorySource,
        AlertMarginYLgFactorySource,
        AlertMarginYXlFactorySource,
    ]}>
    <div id="tab-1" data-label="传统写法">
        <AlertMarginYXs />
