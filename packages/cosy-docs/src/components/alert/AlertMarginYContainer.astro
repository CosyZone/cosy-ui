import AlertMarginYXs from './AlertMarginYXs.astro';
import AlertMarginYSm from './AlertMarginYSm.astro';
import AlertMarginYMd from './AlertMarginYMd.astro';
import AlertMarginYLg from './AlertMarginYLg.astro';
import AlertMarginYXl from './AlertMarginYXl.astro';
import AlertMarginYXsFactory from './AlertMarginYXsFactory.astro';
import AlertMarginYSmFactory from './AlertMarginYSmFactory.astro';
import AlertMarginYMdFactory from './AlertMarginYMdFactory.astro';
import AlertMarginYLgFactory from './AlertMarginYLgFactory.astro';
import AlertMarginYXlFactory from './AlertMarginYXlFactory.astro';
