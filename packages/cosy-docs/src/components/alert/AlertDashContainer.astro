---

/**
 * @component AlertDashContainer
 * @description 展示 Alert 组件 dash 虚线描边风格的示例
 */
import { CodeContainer } from '@coffic/cosy-ui';
import AlertDash from './AlertDash.astro';
import AlertDashSource from './AlertDash.astro?raw';
import AlertDashVue from './AlertDashVue.vue';
import AlertDashVueSource from './AlertDashVue.vue?raw';
---

<CodeContainer
    codes={[
        AlertDashSource,
        AlertDashVueSource,
    ]}
    titles={[
        'Dash 风格',
        'Dash 风格',
    ]}>
    <div id="tab-1" data-label="astro">
        <AlertDash />
    </div>
    <div id="tab-2" data-label="vue">
        <AlertDashVue client:load />
    </div>
</CodeContainer>
