import ContainerWithSlotSourceCode from "./ContainerWithSlot.astro?raw";
import ContainerWithSlotVue from "./ContainerWithSlotVue.vue";
import ContainerWithSlotVueSourceCode from "./ContainerWithSlotVue.vue?raw";
---

<CodeContainer titles={['default slot', 'default slot']} codes={[ContainerWithSlotSourceCode, ContainerWithSlotVueSourceCode]}>
