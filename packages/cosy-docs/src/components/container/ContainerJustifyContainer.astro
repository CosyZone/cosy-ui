---



/**
 * @component ContainerJustifyContainer
 * @description Container 组件垂直对齐示例容器
 */
import { CodeContainer } from "@coffic/cosy-ui";
import ContainerJustifyAround from "./ContainerJustifyAround.astro";
import sourceAround from "./ContainerJustifyAround.astro?raw";
import ContainerJustifyBetween from "./ContainerJustifyBetween.astro";
import sourceBetween from "./ContainerJustifyBetween.astro?raw";
import ContainerJustifyCenter from "./ContainerJustifyCenter.astro";
import sourceCenter from "./ContainerJustifyCenter.astro?raw";
import ContainerJustifyEnd from "./ContainerJustifyEnd.astro";
import sourceEnd from "./ContainerJustifyEnd.astro?raw";
import ContainerJustifyEvenly from "./ContainerJustifyEvenly.astro";
import sourceEvenly from "./ContainerJustifyEvenly.astro?raw";
import ContainerJustifyStart from "./ContainerJustifyStart.astro";
import sourceStart from "./ContainerJustifyStart.astro?raw";
---

<CodeContainer
  codes={[
    sourceStart,
    sourceCenter,
    sourceEnd,
    sourceBetween,
    sourceAround,
    sourceEvenly,
  ]}
  titles={['start', 'center', 'end', 'between', 'around', 'evenly']}>
  <div id="tab-1">
    <ContainerJustifyStart />
  </div>
  <div id="tab-2">
    <ContainerJustifyCenter />
  </div>
  <div id="tab-3">
    <ContainerJustifyEnd />
  </div>
  <div id="tab-4">
    <ContainerJustifyBetween />
  </div>
  <div id="tab-5">
    <ContainerJustifyAround />
  </div>
  <div id="tab-6">
    <ContainerJustifyEvenly />
  </div>
</CodeContainer>
