---
/**
 * @component ContainerFlexBetween
 * @description Container 组件两端对齐示例
 */
import { Container } from '@coffic/cosy-ui';
---

<Container flex="row" gap="md" justify="between" class="cosy:bg-base-200">
  <div class="cosy:bg-primary cosy:text-primary-content cosy:p-2 cosy:rounded">
    左侧
  </div>
  <div
    class="cosy:bg-secondary cosy:text-secondary-content cosy:p-2 cosy:rounded">
    右侧
  </div>
</Container>
