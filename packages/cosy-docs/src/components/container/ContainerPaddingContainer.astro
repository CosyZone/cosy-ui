---
/**
 * @component ContainerPaddingContainer
 * @description 展示 Container 组件的 padding 属性所有取值，Astro/Vue 各自独立标签展示。
 */
import { CodeContainer } from '@coffic/cosy-ui';
import ContainerPaddingLg from './ContainerPaddingLg.astro';
import ContainerPaddingLgRaw from './ContainerPaddingLg.astro?raw';
import ContainerPaddingLgVue from './ContainerPaddingLgVue.vue';
import ContainerPaddingLgVueRaw from './ContainerPaddingLgVue.vue?raw';
import ContainerPaddingMd from './ContainerPaddingMd.astro';
import ContainerPaddingMdRaw from './ContainerPaddingMd.astro?raw';
import ContainerPaddingMdVue from './ContainerPaddingMdVue.vue';
import ContainerPaddingMdVueRaw from './ContainerPaddingMdVue.vue?raw';
import ContainerPaddingNone from './ContainerPaddingNone.astro';
import ContainerPaddingNoneRaw from './ContainerPaddingNone.astro?raw';
import ContainerPaddingNoneVue from './ContainerPaddingNoneVue.vue';
import ContainerPaddingNoneVueRaw from './ContainerPaddingNoneVue.vue?raw';
import ContainerPaddingSm from './ContainerPaddingSm.astro';
import ContainerPaddingSmRaw from './ContainerPaddingSm.astro?raw';
import ContainerPaddingSmVue from './ContainerPaddingSmVue.vue';
import ContainerPaddingSmVueRaw from './ContainerPaddingSmVue.vue?raw';
import ContainerPaddingXl from './ContainerPaddingXl.astro';
import ContainerPaddingXlRaw from './ContainerPaddingXl.astro?raw';
import ContainerPaddingXlVue from './ContainerPaddingXlVue.vue';
import ContainerPaddingXlVueRaw from './ContainerPaddingXlVue.vue?raw';
---

<CodeContainer
	codes={[
		ContainerPaddingNoneRaw,
		ContainerPaddingSmRaw,
		ContainerPaddingMdRaw,
		ContainerPaddingLgRaw,
		ContainerPaddingXlRaw,
		ContainerPaddingNoneVueRaw,
		ContainerPaddingSmVueRaw,
		ContainerPaddingMdVueRaw,
		ContainerPaddingLgVueRaw,
		ContainerPaddingXlVueRaw,
	]}
	titles={['none', 'sm', 'md', 'lg', 'xl', 'none', 'sm', 'md', 'lg', 'xl']}
>
	<div id="tab-1" data-label="astro"><ContainerPaddingNone /></div>
	<div id="tab-2" data-label="astro"><ContainerPaddingSm /></div>
	<div id="tab-3" data-label="astro"><ContainerPaddingMd /></div>
	<div id="tab-4" data-label="astro"><ContainerPaddingLg /></div>
	<div id="tab-5" data-label="astro"><ContainerPaddingXl /></div>
	<div id="tab-6" data-label="vue"><ContainerPaddingNoneVue /></div>
	<div id="tab-7" data-label="vue"><ContainerPaddingSmVue /></div>
	<div id="tab-8" data-label="vue"><ContainerPaddingMdVue /></div>
	<div id="tab-9" data-label="vue"><ContainerPaddingLgVue /></div>
	<div id="tab-10" data-label="vue"><ContainerPaddingXlVue /></div>
</CodeContainer>
