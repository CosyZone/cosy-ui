---

/**
 * @component ContainerPaddingContainer
 * @description Container 组件内边距多标签演示容器
 */
import { CodeContainer } from "@coffic/cosy-ui";
import ContainerPaddingLg from "./ContainerPaddingLg.astro";
import sourceLg from "./ContainerPaddingLg.astro?raw";
import ContainerPaddingMd from "./ContainerPaddingMd.astro";
import sourceMd from "./ContainerPaddingMd.astro?raw";
import ContainerPaddingNone from "./ContainerPaddingNone.astro";
import sourceNone from "./ContainerPaddingNone.astro?raw";
import ContainerPaddingSm from "./ContainerPaddingSm.astro";
import sourceSm from "./ContainerPaddingSm.astro?raw";
import ContainerPaddingXl from "./ContainerPaddingXl.astro";
import sourceXl from "./ContainerPaddingXl.astro?raw";
---

<CodeContainer
  titles={['none', 'sm', 'md', 'lg', 'xl']}
  codes={[sourceNone, sourceSm, sourceMd, sourceLg, sourceXl]}>
  <div id="tab-1"><ContainerPaddingNone /></div>
  <div id="tab-2"><ContainerPaddingSm /></div>
  <div id="tab-3"><ContainerPaddingMd /></div>
  <div id="tab-4"><ContainerPaddingLg /></div>
  <div id="tab-5"><ContainerPaddingXl /></div>
</CodeContainer>
