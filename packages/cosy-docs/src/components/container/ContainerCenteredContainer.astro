---
/**
 * @component ContainerCenteredContainer
 * @description Container 组件居中示例容器
 */
import { CodeContainer } from '@coffic/cosy-ui';
import ContainerCenteredTrue from './ContainerCenteredTrue.astro';
import ContainerCenteredFalse from './ContainerCenteredFalse.astro';
import sourceTrue from './ContainerCenteredTrue.astro?raw';
import sourceFalse from './ContainerCenteredFalse.astro?raw';
---

<CodeContainer codes={[sourceTrue, sourceFalse]} titles={['true', 'false']}>
  <div id="tab-1">
    <ContainerCenteredTrue />
  </div>
  <div id="tab-2">
    <ContainerCenteredFalse />
  </div>
</CodeContainer>
