---

/**
 * @component ContainerBorderContainer
 * @description Container 组件边框示例容器
 */
import { CodeContainer } from "@coffic/cosy-ui";
import ContainerBorderLg from "./ContainerBorderLg.astro";
import sourceLg from "./ContainerBorderLg.astro?raw";
import ContainerBorderMd from "./ContainerBorderMd.astro";
import sourceMd from "./ContainerBorderMd.astro?raw";
import ContainerBorderNone from "./ContainerBorderNone.astro";
import sourceNone from "./ContainerBorderNone.astro?raw";
import ContainerBorderSm from "./ContainerBorderSm.astro";
import sourceSm from "./ContainerBorderSm.astro?raw";
import ContainerBorderXl from "./ContainerBorderXl.astro";
import sourceXl from "./ContainerBorderXl.astro?raw";
---

<CodeContainer
    codes={[sourceNone, sourceSm, sourceMd, sourceLg, sourceXl]}
    titles={['none', 'sm', 'md', 'lg', 'xl']}>
    <div id="tab-1">
        <ContainerBorderNone />
    </div>
    <div id="tab-2">
        <ContainerBorderSm />
    </div>
    <div id="tab-3">
        <ContainerBorderMd />
    </div>
    <div id="tab-4">
        <ContainerBorderLg />
    </div>
    <div id="tab-5">
        <ContainerBorderXl />
    </div>
</CodeContainer>
