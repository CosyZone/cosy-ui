 * @component ContainerAspectRatioContainer
 * @description Container 组件宽高比多标签演示容器
 */
import { CodeContainer } from "@coffic/cosy-ui";
import ContainerAspectRatio1x1 from "./ContainerAspectRatio1x1.astro";
import source1x1 from "./ContainerAspectRatio1x1.astro?raw";
import ContainerAspectRatio3x2 from "./ContainerAspectRatio3x2.astro";
import source3x2 from "./ContainerAspectRatio3x2.astro?raw";
import ContainerAspectRatio4x3 from "./ContainerAspectRatio4x3.astro";
import source4x3 from "./ContainerAspectRatio4x3.astro?raw";
import ContainerAspectRatio16x9 from "./ContainerAspectRatio16x9.astro";
import source16x9 from "./ContainerAspectRatio16x9.astro?raw";
import ContainerAspectRatioConflict from "./ContainerAspectRatioConflict.astro";
import sourceConflict from "./ContainerAspectRatioConflict.astro?raw";
import ContainerAspectRatioWithHeight from "./ContainerAspectRatioWithHeight.astro";
import sourceWithHeight from "./ContainerAspectRatioWithHeight.astro?raw";
import ContainerAspectRatioWithWidth from "./ContainerAspectRatioWithWidth.astro";
import sourceWithWidth from "./ContainerAspectRatioWithWidth.astro?raw";
---

<CodeContainer
  titles={[
    '16:9',
    '4:3',
    '1:1',
    '3:2',
    'aspectRatio + height',
    'aspectRatio + width',
    '冲突示例',
