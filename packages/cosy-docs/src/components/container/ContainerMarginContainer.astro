import ContainerMarginXs from "./ContainerMarginXs.astro";
import sourceXs from "./ContainerMarginXs.astro?raw";
---

<CodeContainer
  titles={[
    'none',
    'xs',
    'sm',
    'md',
    'lg',
    'xl',
    '2xl',
    '3xl',
    '4xl',
    '5xl',
    '6xl',
  ]}
  codes={[
    sourceNone,
    sourceXs,
    sourceSm,
    sourceMd,
    sourceLg,
    sourceXl,
    source2xl,
    source3xl,
    source4xl,
    source5xl,
    source6xl,
  ]}>
  <div id="tab-1"><ContainerMarginNone /></div>
  <div id="tab-2"><ContainerMarginXs /></div>
  <div id="tab-3"><ContainerMarginSm /></div>
  <div id="tab-4"><ContainerMarginMd /></div>
  <div id="tab-5"><ContainerMarginLg /></div>
  <div id="tab-6"><ContainerMarginXl /></div>
  <div id="tab-7"><ContainerMargin2xl /></div>
  <div id="tab-8"><ContainerMargin3xl /></div>
  <div id="tab-9"><ContainerMargin4xl /></div>
  <div id="tab-10"><ContainerMargin5xl /></div>
  <div id="tab-11"><ContainerMargin6xl /></div>
