---
/**
 * @component ContainerMarginContainer
 * @description Container 组件外边距多标签演示容器
 */
import { CodeContainer } from '@coffic/cosy-ui';
import ContainerMarginNone from './ContainerMarginNone.astro';
import ContainerMarginXs from './ContainerMarginXs.astro';
import ContainerMarginSm from './ContainerMarginSm.astro';
import ContainerMarginMd from './ContainerMarginMd.astro';
import ContainerMarginLg from './ContainerMarginLg.astro';
import ContainerMarginXl from './ContainerMarginXl.astro';
import ContainerMargin2xl from './ContainerMargin2xl.astro';
import ContainerMargin3xl from './ContainerMargin3xl.astro';
import ContainerMargin4xl from './ContainerMargin4xl.astro';
import ContainerMargin5xl from './ContainerMargin5xl.astro';
import ContainerMargin6xl from './ContainerMargin6xl.astro';
import sourceNone from './ContainerMarginNone.astro?raw';
import sourceXs from './ContainerMarginXs.astro?raw';
import sourceSm from './ContainerMarginSm.astro?raw';
import sourceMd from './ContainerMarginMd.astro?raw';
import sourceLg from './ContainerMarginLg.astro?raw';
import sourceXl from './ContainerMarginXl.astro?raw';
import source2xl from './ContainerMargin2xl.astro?raw';
import source3xl from './ContainerMargin3xl.astro?raw';
import source4xl from './ContainerMargin4xl.astro?raw';
import source5xl from './ContainerMargin5xl.astro?raw';
import source6xl from './ContainerMargin6xl.astro?raw';
---

<CodeContainer
  titles={[
    'none',
    'xs',
    'sm',
    'md',
    'lg',
    'xl',
    '2xl',
    '3xl',
    '4xl',
    '5xl',
    '6xl',
  ]}
  codes={[
    sourceNone,
    sourceXs,
    sourceSm,
    sourceMd,
    sourceLg,
    sourceXl,
    source2xl,
    source3xl,
    source4xl,
    source5xl,
    source6xl,
  ]}>
  <div id="tab-1"><ContainerMarginNone /></div>
  <div id="tab-2"><ContainerMarginXs /></div>
  <div id="tab-3"><ContainerMarginSm /></div>
  <div id="tab-4"><ContainerMarginMd /></div>
  <div id="tab-5"><ContainerMarginLg /></div>
  <div id="tab-6"><ContainerMarginXl /></div>
  <div id="tab-7"><ContainerMargin2xl /></div>
  <div id="tab-8"><ContainerMargin3xl /></div>
  <div id="tab-9"><ContainerMargin4xl /></div>
  <div id="tab-10"><ContainerMargin5xl /></div>
  <div id="tab-11"><ContainerMargin6xl /></div>
</CodeContainer>
