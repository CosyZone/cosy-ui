---

/**
 * @component ContainerGapContainer
 * @description Container 组件间距示例容器
 */
import { CodeContainer } from "@coffic/cosy-ui";
import ContainerGapLg from "./ContainerGapLg.astro";
import sourceLg from "./ContainerGapLg.astro?raw";
import ContainerGapMd from "./ContainerGapMd.astro";
import sourceMd from "./ContainerGapMd.astro?raw";
import ContainerGapNone from "./ContainerGapNone.astro";
import sourceNone from "./ContainerGapNone.astro?raw";
import ContainerGapSm from "./ContainerGapSm.astro";
import sourceSm from "./ContainerGapSm.astro?raw";
import ContainerGapXl from "./ContainerGapXl.astro";
import sourceXl from "./ContainerGapXl.astro?raw";
---

<CodeContainer
  codes={[sourceNone, sourceSm, sourceMd, sourceLg, sourceXl]}
  titles={['none', 'sm', 'md', 'lg', 'xl']}>
  <div id="tab-1">
    <ContainerGapNone />
  </div>
  <div id="tab-2">
    <ContainerGapSm />
  </div>
  <div id="tab-3">
    <ContainerGapMd />
  </div>
  <div id="tab-4">
    <ContainerGapLg />
  </div>
  <div id="tab-5">
    <ContainerGapXl />
  </div>
</CodeContainer>
