---

/**
 * @component ContainerItemsContainer
 * @description Container 组件对齐方式示例容器
 */
import { CodeContainer } from "@coffic/cosy-ui";
import ContainerItemsBaseline from "./ContainerItemsBaseline.astro";
import sourceBaseline from "./ContainerItemsBaseline.astro?raw";
import ContainerItemsCenter from "./ContainerItemsCenter.astro";
import sourceCenter from "./ContainerItemsCenter.astro?raw";
import ContainerItemsEnd from "./ContainerItemsEnd.astro";
import sourceEnd from "./ContainerItemsEnd.astro?raw";
import ContainerItemsStart from "./ContainerItemsStart.astro";
import sourceStart from "./ContainerItemsStart.astro?raw";
import ContainerItemsStretch from "./ContainerItemsStretch.astro";
import sourceStretch from "./ContainerItemsStretch.astro?raw";
---

<CodeContainer
  codes={[sourceStart, sourceCenter, sourceEnd, sourceStretch, sourceBaseline]}
  titles={['start', 'center', 'end', 'stretch', 'baseline']}>
  <div id="tab-1">
    <ContainerItemsStart />
  </div>
  <div id="tab-2">
    <ContainerItemsCenter />
  </div>
  <div id="tab-3">
    <ContainerItemsEnd />
  </div>
  <div id="tab-4">
    <ContainerItemsStretch />
  </div>
  <div id="tab-5">
    <ContainerItemsBaseline />
  </div>
</CodeContainer>
