---
/**
 * @component ContainerCentered
 * @description Container 组件居中示例
 */
import { Container } from '@coffic/cosy-ui';
---

<div style="display: flex; flex-direction: column; gap: 1rem;">
  <Container centered={true} style="background-color: #f1f5f9;">
    <p style="text-align: center;">居中的容器</p>
  </Container>

  <Container centered={false} style="background-color: #f1f5f9;">
    <p style="text-align: center;">不居中的容器</p>
  </Container>
</div>
