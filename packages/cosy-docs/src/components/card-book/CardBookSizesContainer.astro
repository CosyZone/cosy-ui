---



import { CodeContainer } from "@coffic/cosy-ui";
import CardBookSizeLg from "./CardBookSizeLg.astro";
import CardBookSizeLgSource from "./CardBookSizeLg.astro?raw";
import CardBookSizeMd from "./CardBookSizeMd.astro";
import CardBookSizeMdSource from "./CardBookSizeMd.astro?raw";
import CardBookSizeSm from "./CardBookSizeSm.astro";
import CardBookSizeSmSource from "./CardBookSizeSm.astro?raw";
---

<CodeContainer
  codes={[CardBookSizeSmSource, CardBookSizeMdSource, CardBookSizeLgSource]}
  titles={['sm', 'md', 'lg']}>
  <div id="tab-1">
    <CardBookSizeSm />
  </div>
  <div id="tab-2">
    <CardBookSizeMd />
  </div>
  <div id="tab-3">
    <CardBookSizeLg />
  </div>
</CodeContainer>
