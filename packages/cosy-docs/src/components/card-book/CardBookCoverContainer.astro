---
import { CodeContainer } from '@coffic/cosy-ui';
import CardBookCover from './CardBookCover.astro';
import CardBookCoverNone from './CardBookCoverNone.astro';

import CardBookCoverSource from './CardBookCover.astro?raw';
import CardBookCoverNoneSource from './CardBookCoverNone.astro?raw';
---

<CodeContainer
  codes={[CardBookCoverSource, CardBookCoverNoneSource]}
  titles={['带封面', '无封面']}>
  <div id="tab-1">
    <CardBookCover />
  </div>
  <div id="tab-2">
    <CardBookCoverNone />
  </div>
</CodeContainer>
