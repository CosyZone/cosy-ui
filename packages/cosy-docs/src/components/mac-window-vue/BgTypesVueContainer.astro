---
/**
 * @component MacWindow BgTypes Vue Container
 * @description MacWindow 背景色类型 Vue 容器组件
 */

import { CodeContainer } from "@coffic/cosy-ui";
import DefaultBgVue from "./DefaultBg.vue";
import PrimaryBgVue from "./PrimaryBg.vue";
import SecondaryBgVue from "./SecondaryBg.vue";
import AccentBgVue from "./AccentBg.vue";
import NeutralBgVue from "./NeutralBg.vue";
import InfoBgVue from "./InfoBg.vue";
import SuccessBgVue from "./SuccessBg.vue";
import WarningBgVue from "./WarningBg.vue";
import ErrorBgVue from "./ErrorBg.vue";
import DefaultBgVueSource from "./DefaultBg.vue?raw";
import PrimaryBgVueSource from "./PrimaryBg.vue?raw";
import SecondaryBgVueSource from "./SecondaryBg.vue?raw";
import AccentBgVueSource from "./AccentBg.vue?raw";
import NeutralBgVueSource from "./NeutralBg.vue?raw";
import InfoBgVueSource from "./InfoBg.vue?raw";
import SuccessBgVueSource from "./SuccessBg.vue?raw";
import WarningBgVueSource from "./WarningBg.vue?raw";
import ErrorBgVueSource from "./ErrorBg.vue?raw";
---

<CodeContainer
  titles={[
    '默认',
    '主要',
    '次要',
    '强调',
    '中性',
    '信息',
    '成功',
    '警告',
    '错误',
  ]}
  codes={[
    DefaultBgVueSource,
    PrimaryBgVueSource,
    SecondaryBgVueSource,
    AccentBgVueSource,
    NeutralBgVueSource,
    InfoBgVueSource,
    SuccessBgVueSource,
    WarningBgVueSource,
    ErrorBgVueSource,
  ]}>
  <div id="tab-1">
    <DefaultBgVue client:load />
  </div>
  <div id="tab-2">
    <PrimaryBgVue client:load />
  </div>
  <div id="tab-3">
    <SecondaryBgVue client:load />
  </div>
  <div id="tab-4">
    <AccentBgVue client:load />
  </div>
  <div id="tab-5">
    <NeutralBgVue client:load />
  </div>
  <div id="tab-6">
    <InfoBgVue client:load />
  </div>
  <div id="tab-7">
    <SuccessBgVue client:load />
  </div>
  <div id="tab-8">
    <WarningBgVue client:load />
  </div>
  <div id="tab-9">
    <ErrorBgVue client:load />
  </div>
</CodeContainer>
