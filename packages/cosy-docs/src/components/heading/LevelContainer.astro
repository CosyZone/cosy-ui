---



/**
 * @component HeadingLevelContainer
 * @description Heading 组件 level 属性示例容器，展示不同级别的标题。
 */
import { CodeContainer } from "@coffic/cosy-ui";
import Level1 from "./Level1.astro";
import Level1Source from "./Level1.astro?raw";
import Level2 from "./Level2.astro";
import Level2Source from "./Level2.astro?raw";
import Level3 from "./Level3.astro";
import Level3Source from "./Level3.astro?raw";
import Level4 from "./Level4.astro";
import Level4Source from "./Level4.astro?raw";
import Level5 from "./Level5.astro";
import Level5Source from "./Level5.astro?raw";
import Level6 from "./Level6.astro";
import Level6Source from "./Level6.astro?raw";
---

<CodeContainer
  codes={[
    Level1Source,
    Level2Source,
    Level3Source,
    Level4Source,
    Level5Source,
    Level6Source,
  ]}
  titles={['一级', '二级', '三级', '四级', '五级', '六级']}>
  <div id="tab-1">
    <Level1 />
  </div>
  <div id="tab-2">
    <Level2 />
  </div>
  <div id="tab-3">
    <Level3 />
  </div>
  <div id="tab-4">
    <Level4 />
  </div>
  <div id="tab-5">
    <Level5 />
  </div>
  <div id="tab-6">
    <Level6 />
  </div>
</CodeContainer>
