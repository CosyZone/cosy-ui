---
/**
 * @component ErrorsBasicContainer
 * @description 错误页面基础示例的容器组件，展示最简单的 ErrorPage 用法，Astro/Vue 各占一标签。
 */
import { CodeContainer } from "@coffic/cosy-ui";
import ErrorPage404 from "./ErrorPageBasic.astro";
import ErrorPage404Raw from "./ErrorPageBasic.astro?raw";
import ErrorPage404Vue from "./ErrorPageBasicVue.vue";
import ErrorPage404VueRaw from "./ErrorPageBasicVue.vue?raw";
---

<CodeContainer
    codes={[ErrorPage404Raw, ErrorPage404VueRaw]}
    titles={['404', '404']}
>
    <div id="tab-1" data-label="astro"><ErrorPage404 /></div>
    <div id="tab-2" data-label="vue"><ErrorPage404Vue /></div>
</CodeContainer>
