---



/**
 * @component ButtonVue Link Container
 * @description ButtonVue 链接容器组件
 */

import { CodeContainer } from "@coffic/cosy-ui";
import ButtonVueLinkExternal from "./ButtonVueLinkExternal.vue";
import ButtonVueLinkExternalCode from "./ButtonVueLinkExternal.vue?raw";
import ButtonVueLinkInternal from "./ButtonVueLinkInternal.vue";
import ButtonVueLinkInternalCode from "./ButtonVueLinkInternal.vue?raw";
import ButtonVueLinkStyle from "./ButtonVueLinkStyle.vue";
import ButtonVueLinkStyleCode from "./ButtonVueLinkStyle.vue?raw";
---

<CodeContainer
  titles={['内部链接', '外部链接', '链接样式']}
  codes={[
    ButtonVueLinkInternalCode,
    ButtonVueLinkExternalCode,
    ButtonVueLinkStyleCode,
  ]}>
  <div id="tab-1">
    <ButtonVueLinkInternal client:load />
  </div>
  <div id="tab-2">
    <ButtonVueLinkExternal client:load />
  </div>
  <div id="tab-3">
    <ButtonVueLinkStyle client:load />
  </div>
</CodeContainer>
