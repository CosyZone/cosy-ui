---
/**
 * @component ButtonVue Variants Container
 * @description ButtonVue 变体容器组件
 */

import { CodeContainer } from "@coffic/cosy-ui";
import ButtonVueVariantPrimary from "./ButtonVueVariantPrimary.vue";
import ButtonVueSecondary from "./ButtonVueSecondary.vue";
import ButtonVueAccent from "./ButtonVueAccent.vue";
import ButtonVueVariantInfo from "./ButtonVueVariantInfo.vue";
import ButtonVueVariantSuccess from "./ButtonVueVariantSuccess.vue";
import ButtonVueVariantWarning from "./ButtonVueVariantWarning.vue";
import ButtonVueVariantError from "./ButtonVueVariantError.vue";
import ButtonVueGhost from "./ButtonVueGhost.vue";
import ButtonVueVariantLink from "./ButtonVueVariantLink.vue";
import ButtonVueVariantOutline from "./ButtonVueVariantOutline.vue";
import ButtonVueVariantNeutral from "./ButtonVueVariantNeutral.vue";
import ButtonVueVariantPrimaryCode from "./ButtonVueVariantPrimary.vue?raw";
import ButtonVueSecondaryCode from "./ButtonVueSecondary.vue?raw";
import ButtonVueAccentCode from "./ButtonVueAccent.vue?raw";
import ButtonVueVariantInfoCode from "./ButtonVueVariantInfo.vue?raw";
import ButtonVueVariantSuccessCode from "./ButtonVueVariantSuccess.vue?raw";
import ButtonVueVariantWarningCode from "./ButtonVueVariantWarning.vue?raw";
import ButtonVueVariantErrorCode from "./ButtonVueVariantError.vue?raw";
import ButtonVueGhostCode from "./ButtonVueGhost.vue?raw";
import ButtonVueVariantLinkCode from "./ButtonVueVariantLink.vue?raw";
import ButtonVueVariantOutlineCode from "./ButtonVueVariantOutline.vue?raw";
import ButtonVueVariantNeutralCode from "./ButtonVueVariantNeutral.vue?raw";
---

<CodeContainer
  titles={[
    'Primary',
    'Secondary',
    'Accent',
    'Info',
    'Success',
    'Warning',
    'Error',
    'Ghost',
    'Link',
    'Outline',
    'Neutral',
  ]}
  codes={[
    ButtonVueVariantPrimaryCode,
    ButtonVueSecondaryCode,
    ButtonVueAccentCode,
    ButtonVueVariantInfoCode,
    ButtonVueVariantSuccessCode,
    ButtonVueVariantWarningCode,
    ButtonVueVariantErrorCode,
    ButtonVueGhostCode,
    ButtonVueVariantLinkCode,
    ButtonVueVariantOutlineCode,
    ButtonVueVariantNeutralCode,
  ]}>
  <div id="tab-1">
    <ButtonVueVariantPrimary client:load />
  </div>
  <div id="tab-2">
    <ButtonVueSecondary client:load />
  </div>
  <div id="tab-3">
    <ButtonVueAccent client:load />
  </div>
  <div id="tab-4">
    <ButtonVueVariantInfo client:load />
  </div>
  <div id="tab-5">
    <ButtonVueVariantSuccess client:load />
  </div>
  <div id="tab-6">
    <ButtonVueVariantWarning client:load />
  </div>
  <div id="tab-7">
    <ButtonVueVariantError client:load />
  </div>
  <div id="tab-8">
    <ButtonVueGhost client:load />
  </div>
  <div id="tab-9">
    <ButtonVueVariantLink client:load />
  </div>
  <div id="tab-10">
    <ButtonVueVariantOutline client:load />
  </div>
  <div id="tab-11">
    <ButtonVueVariantNeutral client:load />
  </div>
</CodeContainer>
