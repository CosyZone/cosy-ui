---
/**
 * @component ButtonVue Shapes Container
 * @description ButtonVue 形状容器组件
 */

import { CodeContainer } from "@coffic/cosy-ui";
import ButtonVueShapeDefault from "./ButtonVueShapeDefault.vue";
import ButtonVueShapeSquare from "./ButtonVueShapeSquare.vue";
import ButtonVueShapeCircle from "./ButtonVueShapeCircle.vue";
import ButtonVueShapeDefaultCode from "./ButtonVueShapeDefault.vue?raw";
import ButtonVueShapeSquareCode from "./ButtonVueShapeSquare.vue?raw";
import ButtonVueShapeCircleCode from "./ButtonVueShapeCircle.vue?raw";
---

<CodeContainer
  titles={['默认', '方形', '圆形']}
  codes={[
    ButtonVueShapeDefaultCode,
    ButtonVueShapeSquareCode,
    ButtonVueShapeCircleCode,
  ]}>
  <div id="tab-1">
    <ButtonVueShapeDefault client:load />
  </div>
  <div id="tab-2">
    <ButtonVueShapeSquare client:load />
  </div>
  <div id="tab-3">
    <ButtonVueShapeCircle client:load />
  </div>
</CodeContainer>
