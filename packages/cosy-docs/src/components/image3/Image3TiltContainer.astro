---
/**
 * @component Image3TiltContainer
 * @description Image3 组件 tiltAngle 属性示例容器
 */
import { CodeContainer } from '@coffic/cosy-ui';
import Image3Basic from './Image3Basic.astro';
import Image3TiltAngles from './Image3TiltAngles.astro';
import Image3TiltLarge from './Image3TiltLarge.astro';
import Image3BasicSourceCode from './Image3Basic.astro?raw';
import Image3TiltAnglesSourceCode from './Image3TiltAngles.astro?raw';
import Image3TiltLargeSourceCode from './Image3TiltLarge.astro?raw';
---

<CodeContainer
  titles={['默认角度', '小角度', '大角度']}
  codes={[
    Image3BasicSourceCode,
    Image3TiltAnglesSourceCode,
    Image3TiltLargeSourceCode,
  ]}>
  <div id="tab-1"><Image3Basic /></div>
  <div id="tab-2"><Image3TiltAngles /></div>
  <div id="tab-3"><Image3TiltLarge /></div>
</CodeContainer>
