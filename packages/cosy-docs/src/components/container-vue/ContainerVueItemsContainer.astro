---

/**
 * @component ContainerVueItemsContainer
 * @description Container Vue 组件对齐方式示例容器
 */
import { CodeContainer } from "@coffic/cosy-ui";
import ContainerVueItemsBaseline from "./ContainerVueItemsBaseline.vue";
import sourceBaseline from "./ContainerVueItemsBaseline.vue?raw";
import ContainerVueItemsCenter from "./ContainerVueItemsCenter.vue";
import sourceCenter from "./ContainerVueItemsCenter.vue?raw";
import ContainerVueItemsEnd from "./ContainerVueItemsEnd.vue";
import sourceEnd from "./ContainerVueItemsEnd.vue?raw";
import ContainerVueItemsStart from "./ContainerVueItemsStart.vue";
import sourceStart from "./ContainerVueItemsStart.vue?raw";
import ContainerVueItemsStretch from "./ContainerVueItemsStretch.vue";
import sourceStretch from "./ContainerVueItemsStretch.vue?raw";
---

<CodeContainer
    codes={[
        sourceStart,
        sourceCenter,
        sourceEnd,
        sourceStretch,
        sourceBaseline,
    ]}
    titles={['start', 'center', 'end', 'stretch', 'baseline']}>
    <div id="tab-1">
        <ContainerVueItemsStart client:load />
    </div>
    <div id="tab-2">
        <ContainerVueItemsCenter client:load />
    </div>
    <div id="tab-3">
        <ContainerVueItemsEnd client:load />
    </div>
    <div id="tab-4">
        <ContainerVueItemsStretch client:load />
    </div>
    <div id="tab-5">
        <ContainerVueItemsBaseline client:load />
    </div>
</CodeContainer>
