---



/**
 * @component ContainerVue Sizes Container
 * @description ContainerVue 尺寸容器组件
 */

import { CodeContainer } from "@coffic/cosy-ui";
import ContainerVueSizeFull from "./ContainerVueSizeFull.vue";
import ContainerVueSizeFullCode from "./ContainerVueSizeFull.vue?raw";
import ContainerVueSizeLg from "./ContainerVueSizeLg.vue";
import ContainerVueSizeLgCode from "./ContainerVueSizeLg.vue?raw";
import ContainerVueSizeMd from "./ContainerVueSizeMd.vue";
import ContainerVueSizeMdCode from "./ContainerVueSizeMd.vue?raw";
import ContainerVueSizeSm from "./ContainerVueSizeSm.vue";
import ContainerVueSizeSmCode from "./ContainerVueSizeSm.vue?raw";
import ContainerVueSizeXl from "./ContainerVueSizeXl.vue";
import ContainerVueSizeXlCode from "./ContainerVueSizeXl.vue?raw";
import ContainerVueSizeXs from "./ContainerVueSizeXs.vue";
import ContainerVueSizeXsCode from "./ContainerVueSizeXs.vue?raw";
---

<CodeContainer
  titles={['xs', 'sm', 'md', 'lg', 'xl', 'full']}
  codes={[
    ContainerVueSizeXsCode,
    ContainerVueSizeSmCode,
    ContainerVueSizeMdCode,
    ContainerVueSizeLgCode,
    ContainerVueSizeXlCode,
    ContainerVueSizeFullCode,
  ]}>
  <div id="tab-1">
    <ContainerVueSizeXs client:load />
  </div>
  <div id="tab-2">
    <ContainerVueSizeSm client:load />
  </div>
  <div id="tab-3">
    <ContainerVueSizeMd client:load />
  </div>
  <div id="tab-4">
    <ContainerVueSizeLg client:load />
  </div>
  <div id="tab-5">
    <ContainerVueSizeXl client:load />
  </div>
  <div id="tab-6">
    <ContainerVueSizeFull client:load />
  </div>
</CodeContainer>
