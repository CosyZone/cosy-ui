---
import { CodeContainer } from '@coffic/cosy-ui';
import ContainerVueRoundedNone from './ContainerVueRoundedNone.vue';
import ContainerVueRoundedSm from './ContainerVueRoundedSm.vue';
import ContainerVueRoundedMd from './ContainerVueRoundedMd.vue';
import ContainerVueRoundedLg from './ContainerVueRoundedLg.vue';
import ContainerVueRoundedXl from './ContainerVueRoundedXl.vue';
import ContainerVueRoundedFull from './ContainerVueRoundedFull.vue';
import ContainerVueRoundedNoneCode from './ContainerVueRoundedNone.vue?raw';
import ContainerVueRoundedSmCode from './ContainerVueRoundedSm.vue?raw';
import ContainerVueRoundedMdCode from './ContainerVueRoundedMd.vue?raw';
import ContainerVueRoundedLgCode from './ContainerVueRoundedLg.vue?raw';
import ContainerVueRoundedXlCode from './ContainerVueRoundedXl.vue?raw';
import ContainerVueRoundedFullCode from './ContainerVueRoundedFull.vue?raw';
---

<CodeContainer
  codes={[
    ContainerVueRoundedNoneCode,
    ContainerVueRoundedSmCode,
    ContainerVueRoundedMdCode,
    ContainerVueRoundedLgCode,
    ContainerVueRoundedXlCode,
    ContainerVueRoundedFullCode,
  ]}
  titles={['none', 'sm', 'md', 'lg', 'xl', 'full']}>
  <div id="tab-1">
    <ContainerVueRoundedNone client:load />
  </div>
  <div id="tab-2">
    <ContainerVueRoundedSm client:load />
  </div>
  <div id="tab-3">
    <ContainerVueRoundedMd client:load />
  </div>
  <div id="tab-4">
    <ContainerVueRoundedLg client:load />
  </div>
  <div id="tab-5">
    <ContainerVueRoundedXl client:load />
  </div>
  <div id="tab-6">
    <ContainerVueRoundedFull client:load />
  </div>
</CodeContainer>
