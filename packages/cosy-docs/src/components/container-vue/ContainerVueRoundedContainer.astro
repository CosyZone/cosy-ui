---

/**
 * @component ContainerVueRoundedContainer
 * @description Container Vue 组件圆角示例容器
 */
import { CodeContainer } from "@coffic/cosy-ui";
import ContainerVueRoundedFull from "./ContainerVueRoundedFull.vue";
import sourceFull from "./ContainerVueRoundedFull.vue?raw";
import ContainerVueRoundedLg from "./ContainerVueRoundedLg.vue";
import sourceLg from "./ContainerVueRoundedLg.vue?raw";
import ContainerVueRoundedMd from "./ContainerVueRoundedMd.vue";
import sourceMd from "./ContainerVueRoundedMd.vue?raw";
import ContainerVueRoundedNone from "./ContainerVueRoundedNone.vue";
import sourceNone from "./ContainerVueRoundedNone.vue?raw";
import ContainerVueRoundedSm from "./ContainerVueRoundedSm.vue";
import sourceSm from "./ContainerVueRoundedSm.vue?raw";
import ContainerVueRoundedXl from "./ContainerVueRoundedXl.vue";
import sourceXl from "./ContainerVueRoundedXl.vue?raw";
---

<CodeContainer
    codes={[sourceNone, sourceSm, sourceMd, sourceLg, sourceXl, sourceFull]}
    titles={['none', 'sm', 'md', 'lg', 'xl', 'full']}>
    <div id="tab-1">
        <ContainerVueRoundedNone client:load />
    </div>
    <div id="tab-2">
        <ContainerVueRoundedSm client:load />
    </div>
    <div id="tab-3">
        <ContainerVueRoundedMd client:load />
    </div>
    <div id="tab-4">
        <ContainerVueRoundedLg client:load />
    </div>
    <div id="tab-5">
        <ContainerVueRoundedXl client:load />
    </div>
    <div id="tab-6">
        <ContainerVueRoundedFull client:load />
    </div>
</CodeContainer>
