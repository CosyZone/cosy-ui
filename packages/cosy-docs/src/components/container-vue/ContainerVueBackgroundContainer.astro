---

/**
 * @component ContainerVue Background Container
 * @description Container Vue 背景色展示容器组件
 */

import { CodeContainer } from "@coffic/cosy-ui";
import ContainerVueBackgroundAccent from "./ContainerVueBackgroundAccent.vue";
import ContainerVueBackgroundAccentSource from "./ContainerVueBackgroundAccent.vue?raw";
import ContainerVueBackgroundBase from "./ContainerVueBackgroundBase.vue";
import ContainerVueBackgroundBaseSource from "./ContainerVueBackgroundBase.vue?raw";
import ContainerVueBackgroundPrimary from "./ContainerVueBackgroundPrimary.vue";
import ContainerVueBackgroundPrimarySource from "./ContainerVueBackgroundPrimary.vue?raw";
import ContainerVueBackgroundPrimary10 from "./ContainerVueBackgroundPrimary10.vue";
import ContainerVueBackgroundPrimary10Source from "./ContainerVueBackgroundPrimary10.vue?raw";
import ContainerVueBackgroundSuccess from "./ContainerVueBackgroundSuccess.vue";
import ContainerVueBackgroundSuccessSource from "./ContainerVueBackgroundSuccess.vue?raw";
---

<CodeContainer
    titles={[
        '基础背景色',
        '主要背景色',
        '强调背景色',
        '透明度背景色',
        '成功背景色',
    ]}
    codes={[
        ContainerVueBackgroundBaseSource,
        ContainerVueBackgroundPrimarySource,
        ContainerVueBackgroundAccentSource,
        ContainerVueBackgroundPrimary10Source,
        ContainerVueBackgroundSuccessSource,
    ]}>
    <div id="tab-1">
        <ContainerVueBackgroundBase client:load />
    </div>
    <div id="tab-2">
        <ContainerVueBackgroundPrimary client:load />
    </div>
    <div id="tab-3">
        <ContainerVueBackgroundAccent client:load />
    </div>
    <div id="tab-4">
        <ContainerVueBackgroundPrimary10 client:load />
    </div>
    <div id="tab-5">
        <ContainerVueBackgroundSuccess client:load />
    </div>
</CodeContainer>
