---



import { CodeContainer, UserIcon } from "@coffic/cosy-ui";
import code2 from "./examples/class-custom.astro?raw";
import code1 from "./examples/class-default.astro?raw";
---

<CodeContainer codes={[code1, code2]} titles={['默认样式', '自定义样式']}>
  <div id="tab-1">
    <UserIcon size="32px" />
  </div>
  <div id="tab-2">
    <UserIcon
      size="32px"
      class="cosy:text-primary cosy:bg-base-200 cosy:p-2 cosy:rounded-lg"
    />
  </div>
</CodeContainer>
