---
import { CodeContainer } from '@coffic/cosy-ui';
import { UserIcon } from '@coffic/cosy-ui';
import code1 from './examples/color-default.astro?raw';
import code2 from './examples/color-custom.astro?raw';
---

<CodeContainer codes={[code1, code2]} titles={['默认颜色', '自定义颜色']}>
  <div id="tab-1">
    <UserIcon size="32px" />
  </div>
  <div id="tab-2">
    <UserIcon size="32px" color="#3b82f6" />
  </div>
</CodeContainer>
