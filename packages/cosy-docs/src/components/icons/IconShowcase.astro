    <div
      style="text-align: center; padding: 1rem; background: white; border-radius: 0.375rem; border: 1px solid #e5e7eb;">
      <XCircle size="32px" color="#ef4444" />
      <p style="margin-top: 0.5rem; font-size: 0.875rem; color: #6b7280;">
        XCircle
      </p>
    </div>
  </div>
</div>
