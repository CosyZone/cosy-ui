---

import { CodeContainer } from "@coffic/cosy-ui";
// 组件导入
import ShadowNone from "./ShadowNone.astro";
// 源码导入
import ShadowNoneSourceCode from "./ShadowNone.astro?raw";
import ShadowXl from "./ShadowXl.astro";
import ShadowXlSourceCode from "./ShadowXl.astro?raw";
---

<CodeContainer
  codes={[ShadowNoneSourceCode, ShadowXlSourceCode]}
  titles={['none', 'xl']}>
  <div id="tab-1">
    <ShadowNone />
  </div>
  <div id="tab-2">
    <ShadowXl />
  </div>
</CodeContainer>
