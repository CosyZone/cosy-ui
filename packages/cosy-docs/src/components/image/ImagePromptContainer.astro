---
import { CodeContainer } from '@coffic/cosy-ui';
import ImagePromptCode from './ImagePrompt.astro?raw';
import ImagePrompt from './ImagePrompt.astro';
import ImagePromptModel from './ImagePromptModel.astro';
import ImagePromptModelCode from './ImagePromptModel.astro?raw';
import ImagePromptPanelOff from './ImagePromptPanelOff.astro';
import ImagePromptPanelOffCode from './ImagePromptPanelOff.astro?raw';
---

<CodeContainer
  codes={[ImagePromptCode, ImagePromptModelCode, ImagePromptPanelOffCode]}
  titles={['prompt', 'prompt + model', '关闭面板']}>
  <div id="tab-1">
    <ImagePrompt />
  </div>
  <div id="tab-2">
    <ImagePromptModel />
  </div>
  <div id="tab-3">
    <ImagePromptPanelOff />
  </div>
</CodeContainer>
