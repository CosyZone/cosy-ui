---



import { CodeContainer } from "@coffic/cosy-ui";
import ImageBorderLarge from "./ImageBorderLarge.astro";
import ImageBorderLargeCode from "./ImageBorderLarge.astro?raw";
import ImageBorderMedium from "./ImageBorderMedium.astro";
import ImageBorderMediumCode from "./ImageBorderMedium.astro?raw";
import ImageBorderNone from "./ImageBorderNone.astro";
import ImageBorderNoneCode from "./ImageBorderNone.astro?raw";
import ImageBorderSmall from "./ImageBorderSmall.astro";
import ImageBorderSmallCode from "./ImageBorderSmall.astro?raw";
import ImageBorderXl from "./ImageBorderXl.astro";
import ImageBorderXlCode from "./ImageBorderXl.astro?raw";
---

<CodeContainer
  codes={[
    ImageBorderNoneCode,
    ImageBorderSmallCode,
    ImageBorderMediumCode,
    ImageBorderLargeCode,
    ImageBorderXlCode,
  ]}
  titles={['none', 'sm', 'md', 'lg', 'xl']}>
  <div id="tab-1">
    <ImageBorderNone />
  </div>
  <div id="tab-2">
    <ImageBorderSmall />
  </div>
  <div id="tab-3">
    <ImageBorderMedium />
  </div>
  <div id="tab-4">
    <ImageBorderLarge />
  </div>
  <div id="tab-5">
    <ImageBorderXl />
  </div>
</CodeContainer>
