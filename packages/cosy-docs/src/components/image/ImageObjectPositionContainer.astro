---



/**
 * @component ImageObjectPositionContainer
 * @description Image 组件图片位置属性演示容器，展示源码和效果
 */
import { CodeContainer } from "@coffic/cosy-ui";
import ImageObjectPositionBottom from "./ImageObjectPositionBottom.astro";
import ImageObjectPositionBottomSource from "./ImageObjectPositionBottom.astro?raw";
import ImageObjectPositionCenter from "./ImageObjectPositionCenter.astro";
import ImageObjectPositionCenterSource from "./ImageObjectPositionCenter.astro?raw";
import ImageObjectPositionLeft from "./ImageObjectPositionLeft.astro";
import ImageObjectPositionLeftSource from "./ImageObjectPositionLeft.astro?raw";
import ImageObjectPositionRight from "./ImageObjectPositionRight.astro";
import ImageObjectPositionRightSource from "./ImageObjectPositionRight.astro?raw";
import ImageObjectPositionTop from "./ImageObjectPositionTop.astro";
import ImageObjectPositionTopSource from "./ImageObjectPositionTop.astro?raw";
---

<CodeContainer
  codes={[
    ImageObjectPositionCenterSource,
    ImageObjectPositionTopSource,
    ImageObjectPositionBottomSource,
    ImageObjectPositionLeftSource,
    ImageObjectPositionRightSource,
  ]}
  titles={['center', 'top', 'bottom', 'left', 'right']}>
  <div id="tab-1">
    <ImageObjectPositionCenter />
  </div>
  <div id="tab-2">
    <ImageObjectPositionTop />
  </div>
  <div id="tab-3">
    <ImageObjectPositionBottom />
  </div>
  <div id="tab-4">
    <ImageObjectPositionLeft />
  </div>
  <div id="tab-5">
    <ImageObjectPositionRight />
  </div>
</CodeContainer>
