---
/**
 * @component ImageMaskContainer
 * @description Image 组件遮罩形状演示容器，展示多种遮罩效果
 */
import { CodeContainer } from '@coffic/cosy-ui';
import ImageMaskCircle from './ImageMaskCircle.astro';
import ImageMaskCircleSource from './ImageMaskCircle.astro?raw';
import ImageMaskDecagon from './ImageMaskDecagon.astro';
import ImageMaskDecagonSource from './ImageMaskDecagon.astro?raw';
import ImageMaskDiamond from './ImageMaskDiamond.astro';
import ImageMaskDiamondSource from './ImageMaskDiamond.astro?raw';
import ImageMaskHeart from './ImageMaskHeart.astro';
import ImageMaskHeartSource from './ImageMaskHeart.astro?raw';
import ImageMaskHexagon from './ImageMaskHexagon.astro';
import ImageMaskHexagonSource from './ImageMaskHexagon.astro?raw';
import ImageMaskHexagon2 from './ImageMaskHexagon2.astro';
import ImageMaskHexagon2Source from './ImageMaskHexagon2.astro?raw';
import ImageMaskNone from './ImageMaskNone.astro';
import ImageMaskNoneSource from './ImageMaskNone.astro?raw';
import ImageMaskPentagon from './ImageMaskPentagon.astro';
import ImageMaskPentagonSource from './ImageMaskPentagon.astro?raw';
import ImageMaskSquare from './ImageMaskSquare.astro';
import ImageMaskSquareSource from './ImageMaskSquare.astro?raw';
import ImageMaskSquircle from './ImageMaskSquircle.astro';
import ImageMaskSquircleSource from './ImageMaskSquircle.astro?raw';
import ImageMaskStar from './ImageMaskStar.astro';
import ImageMaskStarSource from './ImageMaskStar.astro?raw';
import ImageMaskStar2 from './ImageMaskStar2.astro';
import ImageMaskStar2Source from './ImageMaskStar2.astro?raw';
import ImageMaskTriangle from './ImageMaskTriangle.astro';
import ImageMaskTriangleSource from './ImageMaskTriangle.astro?raw';
import ImageMaskTriangle2 from './ImageMaskTriangle2.astro';
import ImageMaskTriangle2Source from './ImageMaskTriangle2.astro?raw';
import ImageMaskTriangle3 from './ImageMaskTriangle3.astro';
import ImageMaskTriangle3Source from './ImageMaskTriangle3.astro?raw';
import ImageMaskTriangle4 from './ImageMaskTriangle4.astro';
import ImageMaskTriangle4Source from './ImageMaskTriangle4.astro?raw';
---

<CodeContainer
  codes={[
    ImageMaskCircleSource,
    ImageMaskDecagonSource,
    ImageMaskDiamondSource,
    ImageMaskHeartSource,
    ImageMaskHexagonSource,
    ImageMaskHexagon2Source,
    ImageMaskNoneSource,
    ImageMaskPentagonSource,
    ImageMaskSquareSource,
    ImageMaskSquircleSource,
    ImageMaskStarSource,
    ImageMaskStar2Source,
    ImageMaskTriangleSource,
    ImageMaskTriangle2Source,
    ImageMaskTriangle3Source,
    ImageMaskTriangle4Source,
  ]}
  titles={[
    'circle',
    'decagon',
    'diamond',
    'heart',
    'hexagon',
    'hexagon-2',
    'none',
    'pentagon',
    'square',
    'squircle',
    'star',
    'star-2',
    'triangle',
    'triangle-2',
    'triangle-3',
    'triangle-4',
  ]}>
  <div id="tab-1">
    <ImageMaskCircle />
  </div>
  <div id="tab-2">
    <ImageMaskDecagon />
  </div>
  <div id="tab-3">
    <ImageMaskDiamond />
  </div>
  <div id="tab-4">
    <ImageMaskHeart />
  </div>
  <div id="tab-5">
    <ImageMaskHexagon />
  </div>
  <div id="tab-6">
    <ImageMaskHexagon2 />
  </div>
  <div id="tab-7">
    <ImageMaskNone />
  </div>
  <div id="tab-8">
    <ImageMaskPentagon />
  </div>
  <div id="tab-9">
    <ImageMaskSquare />
  </div>
  <div id="tab-10">
    <ImageMaskSquircle />
  </div>
  <div id="tab-11">
    <ImageMaskStar />
  </div>
  <div id="tab-12">
    <ImageMaskStar2 />
  </div>
  <div id="tab-13">
    <ImageMaskTriangle />
  </div>
  <div id="tab-14">
    <ImageMaskTriangle2 />
  </div>
  <div id="tab-15">
    <ImageMaskTriangle3 />
  </div>
  <div id="tab-16">
    <ImageMaskTriangle4 />
  </div>
</CodeContainer>
