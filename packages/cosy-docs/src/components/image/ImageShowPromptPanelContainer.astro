---
import { CodeContainer } from '@coffic/cosy-ui';
import ImagePrompt from './ImagePrompt.astro';
// @ts-ignore
import ImagePromptSourceCode from './ImagePrompt.astro?raw';
import ImagePromptPanelOff from './ImagePromptPanelOff.astro';
// @ts-ignore
import ImagePromptPanelOffSourceCode from './ImagePromptPanelOff.astro?raw';
---

<CodeContainer
  titles={['true', 'false']}
  codes={[ImagePromptSourceCode, ImagePromptPanelOffSourceCode]}>
  <div id="tab-1">
    <ImagePrompt />
  </div>
  <div id="tab-2">
    <ImagePromptPanelOff />
  </div>
</CodeContainer>
