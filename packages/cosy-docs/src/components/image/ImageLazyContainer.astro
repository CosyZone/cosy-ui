---
/**
 * @component ImageLazyContainer
 * @description Image 组件懒加载属性演示容器，展示源码和效果
 */
import { CodeContainer } from '@coffic/cosy-ui';
import ImageLazyTrue from './ImageLazyTrue.astro';
import ImageLazyTrueSource from './ImageLazyTrue.astro?raw';
import ImageLazyFalse from './ImageLazyFalse.astro';
import ImageLazyFalseSource from './ImageLazyFalse.astro?raw';
---

<CodeContainer
  codes={[ImageLazyTrueSource, ImageLazyFalseSource]}
  titles={['true', 'false']}>
  <div id="tab-1">
    <ImageLazyTrue />
  </div>
  <div id="tab-2">
    <ImageLazyFalse />
  </div>
</CodeContainer>
