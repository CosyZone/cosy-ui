 * @component LoginSocialButtonsContainer
 * @description 展示 Login 组件的 socialButtons slot 用法，Astro/Vue 各占一标签。
 */
import { CodeContainer } from "@coffic/cosy-ui";
import LoginSocialButtons from "./LoginSocialButtons.astro";
import LoginSocialButtonsRaw from "./LoginSocialButtons.astro?raw";
import LoginSocialButtonsVue from "./LoginSocialButtonsVue.vue";
