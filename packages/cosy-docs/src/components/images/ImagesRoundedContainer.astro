---
/**
 * @component ImagesRoundedContainer
 * @description Images 组件 rounded 属性示例容器
 */
import { CodeContainer } from '@coffic/cosy-ui';
import ImagesRoundedNone from './ImagesRoundedNone.astro';
import ImagesRoundedMedium from './ImagesRoundedMedium.astro';
import ImagesRoundedLarge from './ImagesRoundedLarge.astro';
import ImagesRoundedFull from './ImagesRoundedFull.astro';
import ImagesRoundedNoneSourceCode from './ImagesRoundedNone.astro?raw';
import ImagesRoundedMediumSourceCode from './ImagesRoundedMedium.astro?raw';
import ImagesRoundedLargeSourceCode from './ImagesRoundedLarge.astro?raw';
import ImagesRoundedFullSourceCode from './ImagesRoundedFull.astro?raw';
---

<CodeContainer
  titles={['无圆角', '中等圆角', '大圆角', '圆形']}
  codes={[
    ImagesRoundedNoneSourceCode,
    ImagesRoundedMediumSourceCode,
    ImagesRoundedLargeSourceCode,
    ImagesRoundedFullSourceCode,
  ]}>
  <div id="tab-1"><ImagesRoundedNone /></div>
  <div id="tab-2"><ImagesRoundedMedium /></div>
  <div id="tab-3"><ImagesRoundedLarge /></div>
  <div id="tab-4"><ImagesRoundedFull /></div>
</CodeContainer>
