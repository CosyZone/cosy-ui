---

import { CodeContainer } from "@coffic/cosy-ui";
import ImageWith from "./ImageWith.astro";
import ImageWithSourceCode from "./ImageWith.astro?raw";
import ImageWithout from "./ImageWithout.astro";
import ImageWithoutSourceCode from "./ImageWithout.astro?raw";
---

<CodeContainer
  titles={['带图片', '无图片']}
  codes={[ImageWithSourceCode, ImageWithoutSourceCode]}>
  <div id="tab-1">
    <ImageWith />
  </div>
  <div id="tab-2">
    <ImageWithout />
  </div>
</CodeContainer>
