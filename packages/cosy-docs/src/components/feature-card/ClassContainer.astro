---

import { CodeContainer } from "@coffic/cosy-ui";
import ClassCustom from "./ClassCustom.astro";
import ClassCustomSourceCode from "./ClassCustom.astro?raw";
import ClassDefault from "./ClassDefault.astro";
import ClassDefaultSourceCode from "./ClassDefault.astro?raw";
---

<CodeContainer
  titles={['默认', '自定义']}
  codes={[ClassDefaultSourceCode, ClassCustomSourceCode]}>
  <div id="tab-1">
    <ClassDefault />
  </div>
  <div id="tab-2">
    <ClassCustom />
  </div>
</CodeContainer>
