---

import { CodeContainer } from "@coffic/cosy-ui";
import PlaceholderVueMuted from "./PlaceholderVueMuted.vue";
import PlaceholderVueMutedSource from "./PlaceholderVueMuted.vue?raw";
import PlaceholderVueNotMuted from "./PlaceholderVueNotMuted.vue";
import PlaceholderVueNotMutedSource from "./PlaceholderVueNotMuted.vue?raw";
---

<CodeContainer
    codes={[PlaceholderVueNotMutedSource, PlaceholderVueMutedSource]}
    titles={['false', 'true']}>
    <div id="tab-1">
        <PlaceholderVueNotMuted />
    </div>
    <div id="tab-2">
        <PlaceholderVueMuted />
    </div>
</CodeContainer>
