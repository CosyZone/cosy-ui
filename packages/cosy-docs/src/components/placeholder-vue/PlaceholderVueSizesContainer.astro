---
/**
 * @component PlaceholderVueSizesContainer
 * @description Vue 版本 Placeholder 组件不同尺寸示例容器
 */
import { CodeContainer } from "@coffic/cosy-ui";
import PlaceholderVueSizeXs from "./PlaceholderVueSizeXs.vue";
import PlaceholderVueSizeSm from "./PlaceholderVueSizeSm.vue";
import PlaceholderVueSizeMd from "./PlaceholderVueSizeMd.vue";
import PlaceholderVueSizeLg from "./PlaceholderVueSizeLg.vue";
import PlaceholderVueSizeXl from "./PlaceholderVueSizeXl.vue";
import PlaceholderVueSize2xl from "./PlaceholderVueSize2xl.vue";
import PlaceholderVueSize3xl from "./PlaceholderVueSize3xl.vue";
import PlaceholderVueSize4xl from "./PlaceholderVueSize4xl.vue";
import PlaceholderVueSize5xl from "./PlaceholderVueSize5xl.vue";
import PlaceholderVueSize6xl from "./PlaceholderVueSize6xl.vue";
import PlaceholderVueSizeFull from "./PlaceholderVueSizeFull.vue";
import PlaceholderVueSizeXsSourceCode from "./PlaceholderVueSizeXs.vue?raw";
import PlaceholderVueSizeSmSourceCode from "./PlaceholderVueSizeSm.vue?raw";
import PlaceholderVueSizeMdSourceCode from "./PlaceholderVueSizeMd.vue?raw";
import PlaceholderVueSizeLgSourceCode from "./PlaceholderVueSizeLg.vue?raw";
import PlaceholderVueSizeXlSourceCode from "./PlaceholderVueSizeXl.vue?raw";
import PlaceholderVueSize2xlSourceCode from "./PlaceholderVueSize2xl.vue?raw";
import PlaceholderVueSize3xlSourceCode from "./PlaceholderVueSize3xl.vue?raw";
import PlaceholderVueSize4xlSourceCode from "./PlaceholderVueSize4xl.vue?raw";
import PlaceholderVueSize5xlSourceCode from "./PlaceholderVueSize5xl.vue?raw";
import PlaceholderVueSize6xlSourceCode from "./PlaceholderVueSize6xl.vue?raw";
import PlaceholderVueSizeFullSourceCode from "./PlaceholderVueSizeFull.vue?raw";
---

<CodeContainer
    titles={[
        'xs',
        'sm',
        'md',
        'lg',
        'xl',
        '2xl',
        '3xl',
        '4xl',
        '5xl',
        '6xl',
        'full',
    ]}
    codes={[
        PlaceholderVueSizeXsSourceCode,
        PlaceholderVueSizeSmSourceCode,
        PlaceholderVueSizeMdSourceCode,
        PlaceholderVueSizeLgSourceCode,
        PlaceholderVueSizeXlSourceCode,
        PlaceholderVueSize2xlSourceCode,
        PlaceholderVueSize3xlSourceCode,
        PlaceholderVueSize4xlSourceCode,
        PlaceholderVueSize5xlSourceCode,
        PlaceholderVueSize6xlSourceCode,
        PlaceholderVueSizeFullSourceCode,
    ]}>
    <div id="tab-1"><PlaceholderVueSizeXs client:load /></div>
    <div id="tab-2"><PlaceholderVueSizeSm client:load /></div>
    <div id="tab-3"><PlaceholderVueSizeMd client:load /></div>
    <div id="tab-4"><PlaceholderVueSizeLg client:load /></div>
    <div id="tab-5"><PlaceholderVueSizeXl client:load /></div>
    <div id="tab-6"><PlaceholderVueSize2xl client:load /></div>
    <div id="tab-7"><PlaceholderVueSize3xl client:load /></div>
    <div id="tab-8"><PlaceholderVueSize4xl client:load /></div>
    <div id="tab-9"><PlaceholderVueSize5xl client:load /></div>
    <div id="tab-10"><PlaceholderVueSize6xl client:load /></div>
    <div id="tab-11"><PlaceholderVueSizeFull client:load /></div>
</CodeContainer>
