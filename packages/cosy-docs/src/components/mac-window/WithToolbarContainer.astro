---
/**
 * @component MacWindow.WithToolbarContainer
 *
 * @description
 * 带工具栏的MacWindow组件示例容器，展示Astro版本。
 */
import { CodeContainer } from "@coffic/cosy-ui";
import WithToolbar from "./WithToolbar.astro";
import WithToolbarSourceCode from "./WithToolbar.astro?raw";
import WithToolbarVue from "./WithToolbar.vue";
import WithToolbarVueSourceCode from "./WithToolbar.vue?raw";
---

<CodeContainer codes={[WithToolbarSourceCode, WithToolbarVueSourceCode]} titles={['工具栏', '工具栏']}>
  <div id="tab-1" data-label="astro">
    <WithToolbar />
  </div>
  <div id="tab-2" data-label="vue">
    <WithToolbarVue />
  </div>
</CodeContainer>
