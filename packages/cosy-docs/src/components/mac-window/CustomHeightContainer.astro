---
/**
 * @component MacWindow.CustomHeightContainer
 *
 * @description
 * 自定义高度的MacWindow组件示例容器，展示Astro和Vue两个版本。
 */
import { CodeContainer } from '@coffic/cosy-ui';
import CustomHeight from './CustomHeight.astro';
import CustomHeightVue from './CustomHeight.vue';
import CustomHeightSourceCode from './CustomHeight.astro?raw';
import CustomHeightVueSourceCode from './CustomHeight.vue?raw';
---

<CodeContainer
  codes={[CustomHeightSourceCode, CustomHeightVueSourceCode]}
  titles={['Astro', 'Vue']}>
  <div id="tab-1">
    <CustomHeight />
  </div>
  <div id="tab-2">
    <CustomHeightVue />
  </div>
</CodeContainer>
