---
/**
 * @component MacWindow.CustomHeightContainer
 *
 * @description
 * 自定义高度的MacWindow组件示例容器，展示Astro版本。
 */
import { CodeContainer } from "@coffic/cosy-ui";
import CustomHeight from "./CustomHeight.astro";
import CustomHeightSourceCode from "./CustomHeight.astro?raw";
import CustomHeightVue from "./CustomHeight.vue";
import CustomHeightVueSourceCode from "./CustomHeight.vue?raw";
---

<CodeContainer codes={[CustomHeightSourceCode, CustomHeightVueSourceCode]} titles={['自定义高度', '自定义高度']}>
  <div id="tab-1" data-label="astro">
    <CustomHeight />
  </div>
  <div id="tab-2" data-label="vue">
    <CustomHeightVue />
  </div>
</CodeContainer>
