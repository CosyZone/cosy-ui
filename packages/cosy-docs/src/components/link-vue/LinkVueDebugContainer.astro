---
/**
 * @component LinkVue Debug Container
 * @description LinkVue 调试模式容器组件
 */

import { CodeContainer } from '@coffic/cosy-ui';
import LinkVueDebugNormal from './LinkVueDebugNormal.vue';
import LinkVueDebugDebug from './LinkVueDebugDebug.vue';
import LinkVueDebugNormalSource from './LinkVueDebugNormal.vue?raw';
import LinkVueDebugDebugSource from './LinkVueDebugDebug.vue?raw';
---

<CodeContainer
  titles={['正常模式', '调试模式']}
  codes={[LinkVueDebugNormalSource, LinkVueDebugDebugSource]}>
  <div id="tab-1">
    <LinkVueDebugNormal client:load />
  </div>
  <div id="tab-2">
    <LinkVueDebugDebug client:load />
  </div>
</CodeContainer>
