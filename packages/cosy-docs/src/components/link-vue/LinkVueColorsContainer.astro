---

/**
 * @component LinkVue Colors Container
 * @description LinkVue 按钮颜色容器组件
 */

import { CodeContainer } from "@coffic/cosy-ui";
import LinkVueColorAccent from "./LinkVueColorAccent.vue";
import LinkVueColorAccentSource from "./LinkVueColorAccent.vue?raw";
import LinkVueColorError from "./LinkVueColorError.vue";
import LinkVueColorErrorSource from "./LinkVueColorError.vue?raw";
import LinkVueColorInfo from "./LinkVueColorInfo.vue";
import LinkVueColorInfoSource from "./LinkVueColorInfo.vue?raw";
import LinkVueColorPrimary from "./LinkVueColorPrimary.vue";
import LinkVueColorPrimarySource from "./LinkVueColorPrimary.vue?raw";
import LinkVueColorSecondary from "./LinkVueColorSecondary.vue";
import LinkVueColorSecondarySource from "./LinkVueColorSecondary.vue?raw";
import LinkVueColorSuccess from "./LinkVueColorSuccess.vue";
import LinkVueColorSuccessSource from "./LinkVueColorSuccess.vue?raw";
import LinkVueColorWarning from "./LinkVueColorWarning.vue";
import LinkVueColorWarningSource from "./LinkVueColorWarning.vue?raw";
---

<CodeContainer
  titles={['主要', '次要', '强调', '信息', '成功', '警告', '错误']}
  codes={[
    LinkVueColorPrimarySource,
    LinkVueColorSecondarySource,
    LinkVueColorAccentSource,
    LinkVueColorInfoSource,
    LinkVueColorSuccessSource,
    LinkVueColorWarningSource,
    LinkVueColorErrorSource,
  ]}>
  <div id="tab-1">
    <LinkVueColorPrimary client:load />
  </div>
  <div id="tab-2">
    <LinkVueColorSecondary client:load />
  </div>
  <div id="tab-3">
    <LinkVueColorAccent client:load />
  </div>
  <div id="tab-4">
    <LinkVueColorInfo client:load />
  </div>
  <div id="tab-5">
    <LinkVueColorSuccess client:load />
  </div>
  <div id="tab-6">
    <LinkVueColorWarning client:load />
  </div>
  <div id="tab-7">
    <LinkVueColorError client:load />
  </div>
</CodeContainer>
