import LinkVueVariantNavSource from './LinkVueVariantNav.vue?raw';
import LinkVueVariantGithubSource from './LinkVueVariantGithub.vue?raw';
---

<CodeContainer
  titles={[
    '默认',
    '主要',
    '次要',
    '文本',
    '行动号召',
    '幽灵',
    '浅色',
    '导航',
    'GitHub',
  ]}
  codes={[
    LinkVueVariantDefaultSource,
    LinkVueVariantPrimarySource,
    LinkVueVariantSecondarySource,
    LinkVueVariantTextSource,
    LinkVueVariantCtaSource,
    LinkVueVariantGhostSource,
    LinkVueVariantLightSource,
    LinkVueVariantNavSource,
    LinkVueVariantGithubSource,
  ]}>
  <div id="tab-1">
    <LinkVueVariantDefault client:load />
  </div>
  <div id="tab-2">
    <LinkVueVariantPrimary client:load />
  </div>
  <div id="tab-3">
    <LinkVueVariantSecondary client:load />
  </div>
  <div id="tab-4">
    <LinkVueVariantText client:load />
  </div>
  <div id="tab-5">
    <LinkVueVariantCta client:load />
  </div>
  <div id="tab-6">
    <LinkVueVariantGhost client:load />
  </div>
  <div id="tab-7">
    <LinkVueVariantLight client:load />
  </div>
  <div id="tab-8">
    <LinkVueVariantNav client:load />
  </div>
  <div id="tab-9">
    <LinkVueVariantGithub client:load />
  </div>
</CodeContainer>
