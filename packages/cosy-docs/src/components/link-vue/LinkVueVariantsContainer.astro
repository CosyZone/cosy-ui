 * @component LinkVue Variants Container
 * @description LinkVue 样式变体容器组件
 */

import { CodeContainer } from '@coffic/cosy-ui';
import LinkVueVariantDefault from './LinkVueVariantDefault.vue';
import LinkVueVariantPrimary from './LinkVueVariantPrimary.vue';
import LinkVueVariantSecondary from './LinkVueVariantSecondary.vue';
import LinkVueVariantText from './LinkVueVariantText.vue';
import LinkVueVariantCta from './LinkVueVariantCta.vue';
import LinkVueVariantGhost from './LinkVueVariantGhost.vue';
import LinkVueVariantLight from './LinkVueVariantLight.vue';
import LinkVueVariantNav from './LinkVueVariantNav.vue';
import LinkVueVariantGithub from './LinkVueVariantGithub.vue';
import LinkVueVariantDefaultSource from './LinkVueVariantDefault.vue?raw';
import LinkVueVariantPrimarySource from './LinkVueVariantPrimary.vue?raw';
import LinkVueVariantSecondarySource from './LinkVueVariantSecondary.vue?raw';
import LinkVueVariantTextSource from './LinkVueVariantText.vue?raw';
