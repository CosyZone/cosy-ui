---

/**
 * @component ApplePhoneVue Slots Container
 * @description ApplePhoneVue 插槽容器组件
 */

import { CodeContainer } from "@coffic/cosy-ui";
import ApplePhoneVueSlotsBasic from "./ApplePhoneVueSlotsBasic.vue";
import ApplePhoneVueSlotsBasicCode from "./ApplePhoneVueSlotsBasic.vue?raw";
import ApplePhoneVueSlotsComplex from "./ApplePhoneVueSlotsComplex.vue";
import ApplePhoneVueSlotsComplexCode from "./ApplePhoneVueSlotsComplex.vue?raw";
import ApplePhoneVueSlotsForm from "./ApplePhoneVueSlotsForm.vue";
import ApplePhoneVueSlotsFormCode from "./ApplePhoneVueSlotsForm.vue?raw";
---

<CodeContainer
  titles={['基础内容', '复杂布局', '表单内容']}
  codes={[
    ApplePhoneVueSlotsBasicCode,
    ApplePhoneVueSlotsComplexCode,
    ApplePhoneVueSlotsFormCode,
  ]}>
  <div id="tab-1">
    <ApplePhoneVueSlotsBasic client:load />
  </div>
  <div id="tab-2">
    <ApplePhoneVueSlotsComplex client:load />
  </div>
  <div id="tab-3">
    <ApplePhoneVueSlotsForm client:load />
  </div>
</CodeContainer>
