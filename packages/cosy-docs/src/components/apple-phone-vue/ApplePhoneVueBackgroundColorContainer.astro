---

/**
 * @component ApplePhoneVue Background Color Container
 * @description ApplePhoneVue 背景色容器组件
 */

import { CodeContainer } from "@coffic/cosy-ui";
import ApplePhoneVueBackgroundColorOpacity from "./ApplePhoneVueBackgroundColorOpacity.vue";
import ApplePhoneVueBackgroundColorOpacityCode from "./ApplePhoneVueBackgroundColorOpacity.vue?raw";
import ApplePhoneVueBackgroundColorPrimary from "./ApplePhoneVueBackgroundColorPrimary.vue";
import ApplePhoneVueBackgroundColorPrimaryCode from "./ApplePhoneVueBackgroundColorPrimary.vue?raw";
import ApplePhoneVueBackgroundColorSuccess from "./ApplePhoneVueBackgroundColorSuccess.vue";
import ApplePhoneVueBackgroundColorSuccessCode from "./ApplePhoneVueBackgroundColorSuccess.vue?raw";
import ApplePhoneVueBackgroundColorWarning from "./ApplePhoneVueBackgroundColorWarning.vue";
import ApplePhoneVueBackgroundColorWarningCode from "./ApplePhoneVueBackgroundColorWarning.vue?raw";
---

<CodeContainer
  titles={['Primary', 'Success', 'Warning', '透明度']}
  codes={[
    ApplePhoneVueBackgroundColorPrimaryCode,
    ApplePhoneVueBackgroundColorSuccessCode,
    ApplePhoneVueBackgroundColorWarningCode,
    ApplePhoneVueBackgroundColorOpacityCode,
  ]}>
  <div id="tab-1">
    <ApplePhoneVueBackgroundColorPrimary client:load />
  </div>
  <div id="tab-2">
    <ApplePhoneVueBackgroundColorSuccess client:load />
  </div>
  <div id="tab-3">
    <ApplePhoneVueBackgroundColorWarning client:load />
  </div>
  <div id="tab-4">
    <ApplePhoneVueBackgroundColorOpacity client:load />
  </div>
</CodeContainer>
