---
import { CodeContainer } from "@coffic/cosy-ui";
import LgExample from "./PhotoWallPaddingLg.astro";
import LgSourceCode from "./PhotoWallPaddingLg.astro?raw";
import MdExample from "./PhotoWallPaddingMd.astro";
import MdSourceCode from "./PhotoWallPaddingMd.astro?raw";
// 组件导入
import NoneExample from "./PhotoWallPaddingNone.astro";
// 源码导入
import NoneSourceCode from "./PhotoWallPaddingNone.astro?raw";
import SmExample from "./PhotoWallPaddingSm.astro";
import SmSourceCode from "./PhotoWallPaddingSm.astro?raw";
import XlExample from "./PhotoWallPaddingXl.astro";
import XlSourceCode from "./PhotoWallPaddingXl.astro?raw";
// Vue 组件导入
import PhotoWallVuePaddingLg from "./PhotoWallVuePaddingLg.vue";
import PhotoWallVuePaddingLgRaw from "./PhotoWallVuePaddingLg.vue?raw";
import PhotoWallVuePaddingMd from "./PhotoWallVuePaddingMd.vue";
import PhotoWallVuePaddingMdRaw from "./PhotoWallVuePaddingMd.vue?raw";
import PhotoWallVuePaddingNone from "./PhotoWallVuePaddingNone.vue";
import PhotoWallVuePaddingNoneRaw from "./PhotoWallVuePaddingNone.vue?raw";
import PhotoWallVuePaddingSm from "./PhotoWallVuePaddingSm.vue";
import PhotoWallVuePaddingSmRaw from "./PhotoWallVuePaddingSm.vue?raw";
import PhotoWallVuePaddingXl from "./PhotoWallVuePaddingXl.vue";
import PhotoWallVuePaddingXlRaw from "./PhotoWallVuePaddingXl.vue?raw";
---

<CodeContainer
  codes={[
    NoneSourceCode,
    SmSourceCode,
    MdSourceCode,
    LgSourceCode,
    XlSourceCode,
    PhotoWallVuePaddingNoneRaw,
    PhotoWallVuePaddingSmRaw,
    PhotoWallVuePaddingMdRaw,
    PhotoWallVuePaddingLgRaw,
    PhotoWallVuePaddingXlRaw,
  ]}
  titles={['none', 'sm', 'md', 'lg', 'xl', 'none', 'sm', 'md', 'lg', 'xl']}>
  <div id="tab-1" data-label="astro">
    <NoneExample />
  </div>
  <div id="tab-2" data-label="astro">
    <SmExample />
  </div>
  <div id="tab-3" data-label="astro">
    <MdExample />
  </div>
  <div id="tab-4" data-label="astro">
    <LgExample />
  </div>
  <div id="tab-5" data-label="astro">
    <XlExample />
  </div>
  <div id="tab-6" data-label="vue">
    <PhotoWallVuePaddingNone />
  </div>
  <div id="tab-7" data-label="vue">
    <PhotoWallVuePaddingSm />
  </div>
  <div id="tab-8" data-label="vue">
    <PhotoWallVuePaddingMd />
  </div>
  <div id="tab-9" data-label="vue">
    <PhotoWallVuePaddingLg />
  </div>
  <div id="tab-10" data-label="vue">
    <PhotoWallVuePaddingXl />
  </div>
</CodeContainer>
