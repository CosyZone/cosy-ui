---

import { CodeContainer, SocialIcon } from "@coffic/cosy-ui";
import Color28a745Code from "./examples/Color28a745.astro?raw";
import Color333Code from "./examples/Color333.astro?raw";
import Color0366d6Code from "./examples/Color0366d6.astro?raw";
import ColorD73a49Code from "./examples/ColorD73a49.astro?raw";
---

<CodeContainer
  codes={[Color333Code, Color0366d6Code, Color28a745Code, ColorD73a49Code]}
  titles={['#333', '#0366d6', '#28a745', '#d73a49']}>
  <div id="tab-1">
    <SocialIcon platform="github" size="24px" color="#333" />
  </div>
  <div id="tab-2">
    <SocialIcon platform="github" size="24px" color="#0366d6" />
  </div>
  <div id="tab-3">
    <SocialIcon platform="github" size="24px" color="#28a745" />
  </div>
  <div id="tab-4">
    <SocialIcon platform="github" size="24px" color="#d73a49" />
  </div>
</CodeContainer>
