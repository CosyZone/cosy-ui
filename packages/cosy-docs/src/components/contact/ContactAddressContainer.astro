---
import { CodeContainer } from '@coffic/cosy-ui';
import ContactWithAddress from './ContactWithAddress.astro';
import ContactWithoutAddress from './ContactWithoutAddress.astro';
import ContactWithAddressSourceCode from './ContactWithAddress.astro?raw';
import ContactWithoutAddressSourceCode from './ContactWithoutAddress.astro?raw';
---

<CodeContainer
  titles={['有地址', '无地址']}
  codes={[ContactWithAddressSourceCode, ContactWithoutAddressSourceCode]}>
  <div id="tab-1">
    <ContactWithAddress />
  </div>
  <div id="tab-2">
    <ContactWithoutAddress />
  </div>
</CodeContainer>
